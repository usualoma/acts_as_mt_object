INSERT INTO `mt_template` VALUES (51,0,0,0,1,1,'2012-02-08 05:45:55','signin',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','„Çµ„Ç§„É≥„Ç§„É≥',NULL,1,'\n<div class=\"widget-sign-in widget\">\n    <h3 class=\"widget-header\">„Çµ„Ç§„É≥„Ç§„É≥</h3>\n    <div id=\"signin-widget-content\" class=\"widget-content\"></div>\n</div>\n<script type=\"text/javascript\">\n/* <![CDATA[ */\nfunction mtUpdateSignInWidget(u) {\n    var separator = \'&nbsp;&nbsp;|&nbsp;&nbsp;\';\n    var el = document.getElementById(\'signin-widget-content\');\n    var content = \'\';\n    if (!el) return;\n    if (u) {\n        if (u && u.is_authenticated) {\n            user = u;\n            mtSaveUser();\n        } else {\n            // user really isn\'t logged in; so let\'s do this!\n            return mtSignIn();\n        }\n    } else {\n        u = mtGetUser();\n    }\n    if (u && u.name) {\n        var url;\n        if (u.is_authenticated) {\n            url = \'<$mt:CGIPath$><$mt:CommunityScript$>?__mode=view&blog_id=<$mt:BlogID$>\';\n            url += \'&static=\' + encodeURIComponent( location.href );\n        } else if (u.url) {\n            url = u.url;\n        } else {\n            url = null;\n        }\n        if (url)\n            content += \'<a href=\"\' + url + \'\">\' + u.name + \'</a>\';\n        else\n            content += \'\' + u.name + \'\';\n        if (u.is_author)\n            content += separator + \'<a href=\"<$mt:CGIPath$><$mt:CommunityScript$>?__mode=edit&blog_id=<$mt:BlogID$>&return_to=\' + encodeURIComponent(document.URL) + \'\">„É¶„Éº„Ç∂„ÉºÊÉÖÂ†±„ÅÆÁ∑®ÈõÜ</a>\';\n        <mt:Unless name=\"hide_signout_link\">\n        content += separator + \'<a href=\"javascript:void(0)\" onclick=\"return mtSignOutOnClick()\">„Çµ„Ç§„É≥„Ç¢„Ç¶„Éà</a>\';\n        </mt:Unless>\n    } else if (u && u.is_banned) {\n        content = \'„Åì„ÅÆ„Éñ„É≠„Ç∞„Å´„Çµ„Ç§„É≥„Ç§„É≥„Åô„ÇãÊ®©Èôê„Åå„ÅÇ„Çä„Åæ„Åõ„Çì„ÄÇ\';\n    } else {\n        content = \'<a href=\"javascript:void(0)\" onclick=\"return mtSignInOnClick(\\\'signin-widget-content\\\')\">„Çµ„Ç§„É≥„Ç§„É≥</a>\';\n        <mt:IfExternalUserManagement>\n        <mt:Else>\n        <mt:IfCommenterRegistrationAllowed>\n        content += separator + \'<a href=\"<$mt:CGIPath$><$mt:CommunityScript$>?__mode=register&blog_id=<$mt:BlogID$>&return_to=\' + encodeURIComponent(document.URL) + \'\">ÁôªÈå≤</a>\';\n        </mt:IfCommenterRegistrationAllowed>\n        </mt:IfExternalUserManagement>\n    }\n    el.innerHTML = content;\n}\nmtAttachEvent(\'usersignin\', mtUpdateSignInWidget);\nmtUpdateSignInWidget();\n/* ]]> */\n</script>\n\n','custom',0);
