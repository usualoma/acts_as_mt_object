INSERT INTO `mt_template` VALUES (1,1,0,0,1,1,'2012-02-08 05:45:55','trackbacks',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','„Éà„É©„ÉÉ„ÇØ„Éê„ÉÉ„ÇØ',NULL,1,'\n<mt:IfPingsActive>\n<div id=\"trackbacks\" class=\"trackbacks\">\n    <h2 class=\"trackbacks-header\"><$mt:EntryTrackbackCount singular=\"„Éà„É©„ÉÉ„ÇØ„Éê„ÉÉ„ÇØ(1)\" plural=\"„Éà„É©„ÉÉ„ÇØ„Éê„ÉÉ„ÇØ(#)\" none=\"„Éà„É©„ÉÉ„ÇØ„Éê„ÉÉ„ÇØ(0)\"$></h2>\n    <mt:IfPingsAccepted>\n    <div class=\"trackbacks-info\">\n        <p>„Éà„É©„ÉÉ„ÇØ„Éê„ÉÉ„ÇØURL: <$mt:EntryTrackbackLink$></p>\n    </div>\n    </mt:IfPingsAccepted>\n    <mt:Pings>\n        <mt:PingsHeader>\n    <div class=\"trackbacks-content\">\n        </mt:PingsHeader>\n        <div class=\"trackback\" id=\"ping-<$mt:PingID$>\">\n            <div class=\"inner\">\n                <div class=\"trackback-header\">\n                    <div class=\"asset-meta\">\n                        <$mt:PingBlogName$> - <a href=\"<$mt:PingURL$>\"><$mt:PingTitle$></a> (<a href=\"#ping-<$mt:PingID$>\"><$mt:PingDate$></a>)\n                    </div>\n                </div>\n                <div class=\"trackback-content\">\n                    <p>\n                        <$mt:PingExcerpt$> <a href=\"<$mt:PingURL$>\">Á∂ö„Åç„ÇíË™≠„ÇÄ</a>\n                    </p>\n                </div>\n            </div>\n        </div>\n        <mt:PingsFooter>\n    </div>\n        </mt:PingsFooter>\n    </mt:Pings>\n</div>\n</mt:IfPingsActive>\n\n\n','custom',0);
INSERT INTO `mt_template` VALUES (2,1,0,0,1,1,'2012-02-08 05:45:55','entry_summary',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','„Éñ„É≠„Ç∞Ë®ò‰∫ã„ÅÆÊ¶ÇË¶Å',NULL,1,'\n<$mt:EntryTrackbackData$>\n<div id=\"entry-<$mt:EntryID$>\" class=\"entry-asset asset hentry\">\n    <div class=\"asset-header\">\n        <h2 class=\"asset-name entry-title\"><a href=\"<$mt:EntryPermalink$>\" rel=\"bookmark\"><$mt:EntryTitle$></a> - <a href=\"<$mt:EntryBlogURL$>\"><$mt:EntryBlogName$></a></h2>\n        <$mt:Include module=\"„Éñ„É≠„Ç∞Ë®ò‰∫ã„ÅÆ„É°„Çø„Éá„Éº„Çø\" blog_id=\"$website_id\"$>\n    </div>\n    <div class=\"asset-content entry-content\">\n<mt:If tag=\"EntryBody\">\n        <div class=\"asset-body\">\n            <$mt:EntryBody$>\n        </div>\n</mt:If>\n<mt:IfTagSearch>\n        <$mt:Include module=\"„Çø„Ç∞\"$>\n</mt:IfTagSearch>\n<mt:If tag=\"EntryMore\" convert_breaks=\"0\">\n        <div class=\"asset-more-link\">\n            Á∂ö„Åç„ÇíË™≠„ÇÄ: <a href=\"<$mt:EntryPermalink$>#more\" rel=\"bookmark\"><$mt:EntryTitle$></a>\n        </div>\n</mt:If>\n    </div>\n    <div class=\"asset-footer\"></div>\n</div>\n\n','custom',0);
