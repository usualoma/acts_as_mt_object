stSubmitted = false;\nfunction mtCommentOnSubmit(f) {\n    if (!mtRequestSubmitted) {\n        mtRequestSubmitted = true;\n\n        if (f.armor)\n            f.armor.value = \'<$mt:BlogSitePath encode_sha1=\"1\"$>\';\n        if (f.bakecookie && f.bakecookie.checked)\n            mtSaveUser(f);\n\n        // disable submit buttons\n        if (f.preview_button) f.preview_button.disabled = true;\n        if (f.post) f.post.disabled = true;\n\n        var u = mtGetUser();\n        if ( !is_preview && ( u && u.is_authenticated ) ) {\n            // validate session; then submit\n            mtFetchedUser = false;\n            mtFetchUser(\'mtCommentSessionVerify\');\n            return false;\n        }\n\n        return true;\n    }\n    return false;\n}\n\nfunction mtCommentSessionVerify(app_user) {\n    var u = mtGetUser();\n    var f = document[\'comments_form\'];\n    if ( u && app_user && app_user.sid && ( u.sid == app_user.sid ) ) {\n        f.submit();\n    } else {\n        alert(\'セッションの有効期限が切れています。再度サインインしてください。\');\n        mtClearUser();\n        mtFireEvent(\'usersignin\');\n<mt:IfRegistrationRequired>\n        mtShow(\'comments-form\');\n        mtHide(\'comments-open-footer\');\n</mt:IfRegistrationRequired>\n    }\n}\n\nfunction mtUserOnLoad() {\n    var u = mtGetUser();\n\n    // if the user is authenticated, hide the \'anonymous\' fields\n    // and any captcha input if already shown\n    if ( document.getElementById(\'comments-form\')) {\n        if ( u && u.is_authenticated ) {\n            mtShow(\'comments-form\');\n            mtHide(\'comments-open-data\');\n            if (mtCaptchaVisible)\n                mtHide(\'comments-open-captcha\');\n        } else {\n<mt:IfRegistrationRequired>\n            mtHide(\'comments-form\');\n</mt:IfRegistrationRequired>\n        }\n        if ( u && u.is_banned )\n            mtHide(\'comments-form\');\n\n        // if we\'re previewing a comment, make sure the captcha\n        // field is visible\n        if (is_preview)\n            mtShowCaptcha();\n        else\n            mtShowGreeting();\n\n        // populate anonymous comment fields if user is cookied as anonymous\n        var cf = document[\'comments_form\'];\n        if (cf) {\n            if (u && u.is_anonymous) {\n                if (u.email) cf.email.value = u.email;\n                if (u.name) cf.author.value = u.name;\n                if (u.url) cf.url.value = u.url;\n                if (cf.bakecookie)\n                    cf.bakecookie.checked = u.name || u.email;\n            } else {\n                if (u && u.sid && cf.sid)\n                    cf.sid.value = u.sid;\n            }\n            if (cf.post && cf.post.disabled)\n                cf.post.disabled = false;\n            if (cf.preview_button && cf.preview_button.disabled)\n                cf.preview_button.disabled = false;\n            mtRequestSubmitted = false;\n        }\n    }\n}\n</mt:IfBlog>\n\n<mt:Ignore>\n/***\n * Called when an entry archive page is loaded.\n * This routine controls which elements of the comment form are shown\n * or hidden, depending on commenter type and blog configuration.\n */\n</mt:Ignore>\n<mt:IfBlog>\nfunction mtEntryOnLoad() {\n    <mt:Unless tag=\"IfPingsAccepted\">mtHide(\'trackbacks-info\');</mt:Unless>\n    <mt:Unless tag=\"IfCommentsAccepted\">mtHide(\'comments-open\');</mt:Unless>\n    mtFireEvent(\'usersignin\');\n}\n\nfunction mtEntryOnUnload() {\n    if (mtRequestSubmitted) {\n        var cf = document[\'comments_form\'];\n        if (cf) {\n            if (cf.post && cf.post.disabled)\n                cf.post.disabled = false;\n            if (cf.preview_button && cf.preview_button.disabled)\n                cf.preview_button.disabled = false;\n        }\n        mtRequestSubmitted = false;\n    }\n    return true;\n}\n\nmtAttachEvent(\'usersignin\', mtUserOnLoad);\n</mt:IfBlog>\n\n<mt:Ignore>\n/***\n * Handles the action of the \"Sign in\" link. First clears any existing\n * user cookie, then directs to the MT comment script to sign the user in.\n */\n</mt:Ignore>\nfunction mtSignIn() {\n    var doc_url = document.URL;\n    doc_url = doc_url.replace(/#.+/, \'\');\n    var url = \'<$mt:CGIPath$><$mt:CommunityScript>?__mode=login&blog_id=<$mt:BlogID$>\';\n    if (is_preview) {\n        if ( document[\'comments_form\'] ) {\n            var entry_id = document[\'comments_form\'].entry_id.value;\n            url += \'&entry_id=\' + entry_id;\n        } else {\n            url += \'&return_url=<$mt:BlogURL encode_url=\"1\"$>\';\n        }\n    } else {\n        url += \'&return_url=\' + encodeURIComponent(doc_url);\n    }\n    mtClearUser();\n    location.href = url;\n}\n\nfunction mtSignInOnClick(sign_in_element) {\n    var el;\n    if (sign_in_element) {\n        // display throbber\n        el = document.getElementById(sign_in_element);\n        if (!el)  // legacy MT 4.x element id\n            el = document.getElementById(\'comment-form-external-auth\');\n    }\n    if (el)\n        el.innerHTML = \'サインインします... <span class=\"status-indicator\">&nbsp;</span>\';\n\n    mtClearUser(); // clear any \'anonymous\' user cookie to allow sign in\n    mtFetchUser(\'mtSetUserOrLogin\');\n    return false;\n}\n\nfunction mtSetUserOrLogin(u) {\n    if (u && u.is_authenticated) {\n        mtSetUser(u);\n    } else {\n        // user really isn\'t logged in; so let\'s do this!\n        mtSignIn();\n    }\n}\n\n<mt:Ignore>\n/***\n * Handles sign out from the web site.\n * First clears any existing user cookie, then direts to the MT comment\n * script to sign the user out.\n */\n</mt:Ignore>\nfunction mtSignOut(entry_id) {\n    mtClearUser();\n    var doc_url = document.URL;\n    doc_url = doc_url.replace(/#.+/, \'\');\n    var url = \'<$mt:CGIPath$><$mt:CommunityScript$>?__mode=logout&blog_id=<$mt:BlogID$>\';\n    if (is_preview) {\n        if ( document[\'comments_form\'] ) {\n            var entry_id = document[\'comments_form\'].entry_id.value;\n            url += \'&entry_id=\' + entry_id;\n        } else {\n            url += \'&return_url=<$mt:BlogURL encode_url=\"1\"$>\';\n        }\n    } else {\n        url += \'&return_url=\' + encodeURIComponent(doc_url);\n    }\n    location.href = url;\n}\n\n<mt:Ignore>\n/***\n * Handles the action of the \"Sign out\" link.\n */\n</mt:Ignore>\nfunction mtSignOutOnClick() {\n    mtSignOut();\n    return false;\n}\n\n<mt:Ignore>\n/***\n * Handles the display of the greeting message, depending on what kind of\n * user is logged in and blog comment policy.\n */\n</mt:Ignore>\n<mt:IfBlog>\nfunction mtShowGreeting() {\n<mt:IfRegistrationAllowed>\n    var reg_reqd = <mt:IfRegistrationRequired>true<mt:Else>false</mt:IfRegistrationRequired>;\n\n    var cf = document[\'comments_form\'];\n    if (!cf) return;\n\n    var el = document.getElementById(\'comment-greeting\');\n    if (!el)  // legacy MT 4.x element id\n        el = document.getElementById(\'comment-form-external-auth\');\n    if (!el) return;\n\n    var eid = cf.entry_id;\n    var entry_id;\n    if (eid) entry_id = eid.value;\n\n    var phrase;\n    var u = mtGetUser();\n\n    if ( u && u.is_authenticated ) {\n        if ( u.is_banned ) {\n            phrase = \'このブログにコメントする権限を持っていません。(\\<a href=\\\"javas\\cript:void(0);\\\" onclick=\\\"return mtSignOutOnClick();\\\"\\>サインアウトする\\<\\/a\\>)\';\n        } else {\n            var user_link;\n            if ( u.is_author ) {\n                user_link = \'<a href=\"<$mt:CGIPath$><$mt:CommunityScript$>?__mode=edit&amp;return_to=\' + encodeURIComponent(document.URL) + \'&amp;blog_id=<$mt:BlogID$>\';\n                user_link += \'\">\' + u.name + \'</a>\';\n            } else {\n                // registered user, but not a user with posting rights\n                if (u.url)\n                    user_link = \'<a href=\"\' + u.url + \'\">\' + u.name + \'</a>\';\n                else\n                    user_link = u.name;\n            }\n            // TBD: supplement phrase with userpic if one is available.\n            phrase = \'__NAME__としてサインインしています。(\\<a href=\\\"javas\\cript:void(0)\\\" onclick=\\\"return mtSignOutOnClick();\\\"\\>サインアウト\\<\\/a\\>)\';\n            phrase = phrase.replace(/__NAME__/, user_link);\n        }\n    } else {\n        if (reg_reqd) {\n            phrase = \'コメントするにはまず\\<a href=\\\"javas\\cript:void(0)\\\" onclick=\\\"return mtSignInOnClick(\\\'comment-greeting\\\')\\\"\\>サインイン\\<\\/a\\>してください。\';\n        } else {\n            phrase = \'コメントする前に\\<a href=\\\"javas\\cript:void(0)\\\" onclick=\\\"return mtSignInOnClick(\\\'comment-greeting\\\')\\\"\\>サインイン\\<\\/a\\>することもできます。\';\n        }\n    }\n    el.innerHTML = phrase;\n<mt:Else>\n    mtShowCaptcha();\n</mt:IfRegistrationAllowed>\n}\n</mt:IfBlog>\n\n<mt:Ignore>\n/***\n * Handles the action of the \'Reply\' links.\n */\n</mt:Ignore>\nfunction mtReplyCommentOnClick(parent_id, author) {\n    mtShow(\'comment-form-reply\');\n\n    var checkbox = document.getElementById(\'comment-reply\');\n    var label = document.getElementById(\'comment-reply-label\');\n    var text = document.getElementById(\'comment-text\');\n\n    // Populate label with new values\n    var reply_text = \'\\<a href=\\\"#comment-__PARENT__\\\" onclick=\\\"location.href=this.href; return false\\\"\\>__AUTHOR__からのコメント\\<\\/a\\>に返信\';\n    reply_text = reply_text.replace(/__PARENT__/, parent_id);\n    reply_text = reply_text.replace(/__AUTHOR__/, author);\n    label.innerHTML = reply_text;\n\n    checkbox.value = parent_id; \n    checkbox.checked = true;\n    try {\n        // text field may be hidden\n        text.focus();\n    } catch(e) {\n    }\n\n    mtSetCommentParentID();\n}\n\n<mt:Ignore>\n/***\n * Sets the parent comment ID when replying to a comment.\n */\n</mt:Ignore>\nfunction mtSetCommentParentID() {\n    var checkbox = document.getElementById(\'comment-reply\');\n    var parent_id_field = document.getElementById(\'comment-parent-id\');\n    if (!checkbox || !parent_id_field) return;\n\n    var pid = 0;\n    if (checkbox.checked == true)\n        pid = checkbox.value;\n    parent_id_field.value = pid;\n}\n\n<mt:Ignore>\n/***\n * Persists a copy of the current user cookie into the browser cookie stash.\n */\n</mt:Ignore>\nfunction mtSaveUser(f) {\n    // We can\'t reliably store the user cookie during a preview.\n    if (is_preview) return;\n\n    var u = mtGetUser();\n\n    if (f && (!u || u.is_anonymous)) {\n        if ( !u ) {\n            u = {};\n            u.is_authenticated = false;\n            u.can_comment = true;\n            u.is_author = false;\n            u.is_banned = false;\n            u.is_anonymous = true;\n            u.is_trusted = false;\n        }\n        if (f.author != undefined) u.name = f.author.value;\n        if (f.email != undefined) u.email = f.email.value;\n        if (f.url != undefined) u.url = f.url.value;\n    }\n\n    if (!u) return;\n\n    var cache_period = mtCookieTimeout * 1000;\n\n    // cache anonymous user info for a long period if the\n    // user has requested to be remembered\n    if (u.is_anonymous && f && f.bakecookie && f.bakecookie.checked)\n        cache_period = 365 * 24 * 60 * 60 * 1000;\n\n    var now = new Date();\n    mtFixDate(now);\n    now.setTime(now.getTime() + cache_period);\n\n    var cmtcookie = mtBakeUserCookie(u);\n    mtSetCookie(mtCookieName, cmtcookie, now, mtCookiePath, mtCookieDomain,\n        location.protocol == \'https:\');\n}\n\n<mt:Ignore>\n/***\n * Clears the blog-side user cookie.\n */\n</mt:Ignore>\nfunction mtClearUser() {\n    user = null;\n    mtDeleteCookie(mtCookieName, mtCookiePath, mtCookieDomain,\n        location.protocol == \'https:\');\n}\n\n<mt:Ignore>\n/***\n * Sets a browser cookie.\n */\n</mt:Ignore>\nfunction mtSetCookie(name, value, expires, path, domain, secure) {\n    if (domain && domain.match(/^\\.?localhost$/))\n        domain = null;\n    var curCookie = name + \"=\" + escape(value) +\n        (expires ? \"; expires=\" + expires.toGMTString() : \"\") +\n        (path ? \"; path=\" + path : \"\") +\n        (domain ? \"; domain=\" + domain : \"\") +\n        (secure ? \"; secure\" : \"\");\n    document.cookie = curCookie;\n}\n\n<mt:Ignore>\n/***\n * Retrieves a browser cookie.\n */\n</mt:Ignore>\nfunction mtGetCookie(name) {\n    var prefix = name + \'=\';\n    var c = document.cookie;\n    var cookieStartIndex = c.indexOf(prefix);\n    if (cookieStartIndex == -1)\n        return \'\';\n    var cookieEndIndex = c.indexOf(\";\", cookieStartIndex + prefix.length);\n    if (cookieEndIndex == -1)\n        cookieEndIndex = c.length;\n    return unescape(c.substring(cookieStartIndex + prefix.length, cookieEndIndex));\n}\n\n<mt:Ignore>\n/***\n * Deletes a browser cookie.\n */\n</mt:Ignore>\nfunction mtDeleteCookie(name, path, domain, secure) {\n    if (mtGetCookie(name)) {\n        if (domain && domain.match(/^\\.?localhost$/))\n            domain = null;\n        document.cookie = name + \"=\" +\n            (path ? \"; path=\" + path : \"\") +\n            (domain ? \"; domain=\" + domain : \"\") +\n            (secure ? \"; secure\" : \"\") +\n            \"; expires=Thu, 01-Jan-70 00:00:01 GMT\";\n    }\n}\n\nfunction mtFixDate(date) {\n    var skew = (new Date(0)).getTime();\n    if (skew > 0)\n        date.setTime(date.getTime() - skew);\n}\n\n<mt:Ignore>\n/***\n * Returns a XMLHttpRequest object (for Ajax operations).\n */\n</mt:Ignore>\nfunction mtGetXmlHttp() {\n    if ( !window.XMLHttpRequest ) {\n        window.XMLHttpRequest = function() {\n            var types = [\n                \"Microsoft.XMLHTTP\",\n                \"MSXML2.XMLHTTP.5.0\",\n                \"MSXML2.XMLHTTP.4.0\",\n                \"MSXML2.XMLHTTP.3.0\",\n                \"MSXML2.XMLHTTP\"\n            ];\n\n            for ( var i = 0; i < types.length; i++ ) {\n                try {\n                    return new ActiveXObject( types[ i ] );\n                } catch( e ) {}\n            }\n\n            return undefined;\n        };\n    }\n    if ( window.XMLHttpRequest )\n        return new XMLHttpRequest();\n}\n\n// BEGIN: fast browser onload init\n// Modifications by David Davis, DWD\n// Dean Edwards/Matthias Miller/John Resig\n// http://dean.edwards.name/weblog/2006/06/again/?full#comment5338\n\nfunction mtInit() {\n    // quit if this function has already been called\n    if (arguments.callee.done) return;\n\n    // flag this function so we don\'t do the same thing twice\n    arguments.callee.done = true;\n\n    // kill the timer\n    // DWD - check against window\n    if ( window._timer ) clearInterval(window._timer);\n\n    // DWD - fire the window onload now, and replace it\n    if ( window.onload && ( window.onload !== window.mtInit ) ) {\n        window.onload();\n        window.onload = function() {};\n    }\n}\n\n/* for Mozilla/Opera9 */\nif (document.addEventListener) {\n    document.addEventListener(\"DOMContentLoaded\", mtInit, false);\n}\n\n/* for Internet Explorer */\n/*@cc_on @*/\n/*@if (@_win32)\ndocument.write(\"<script id=__ie_onload defer src=javascript:void(0)><\\/script>\");\nvar script = document.getElementById(\"__ie_onload\");\nscript.onreadystatechange = function() {\n    if (this.readyState == \"complete\") {\n        mtInit(); // call the onload handler\n    }\n};\n/*@end @*/\n\n/* for Safari */\nif (/WebKit/i.test(navigator.userAgent)) { // sniff\n    _timer = setInterval(function() {\n        if (/loaded|complete/.test(document.readyState)) {\n            mtInit(); // call the onload handler\n        }\n    }, 10);\n}\n\n/* for other browsers */\nwindow.onload = mtInit;\n\n// END: fast browser onload init\n\n<mt:IfBlog>\n<mt:IfRegistrationAllowed>\n/***\n * If request contains a \'#_login\' or \'#_logout\' hash, use this to\n * also delete the blog-side user cookie, since we\'re coming back from\n * a login, logout or edit profile operation.\n */\nvar clearCookie = ( window.location.hash && window.location.hash.match( /^#_log(in|out)/ ) ) ? true : false;\nif (clearCookie) {\n    // clear any logged in state\n    mtClearUser();\n    if (RegExp.$1 == \'in\')\n        mtFetchUser();\n} else {\n    <mt:Ignore>\n    /***\n     * Uncondition this call to fetch the current user state (if available)\n     * from MT upon page load if no user cookie is already present.\n     * This is okay if you have a private install, such as an Intranet;\n     * not recommended for public web sites!\n     */\n    </mt:Ignore>\n    if ( is_preview && !user )\n        mtFetchUser();\n}\n</mt:IfRegistrationAllowed>\n</mt:IfBlog>\n\n','custom',0);
INSERT INTO `mt_template` VALUES (45,0,0,0,1,1,'2012-02-08 05:45:55','new_password_reset_form',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','新しいパスワード再設定フォーム',NULL,1,'\n<$mt:Var name=\"screen_id\" value=\"reset-password\"$>\n<$mt:Var name=\"body_class\" value=\"mt-profile-reset\"$>\n<mt:SetVarBlock name=\"page_title\">パスワードの再設定</mt:SetVarBlock>\n<mt:SetVarBlock name=\"title\"><mt:If name=\"blog_id\"><$mt:BlogName encode_html=\"1\"$>: </mt:If><$mt:Var name=\"page_title\"$></mt:SetVarBlock>\n\n<$mt:Include module=\"シンプルヘッダー\"$>\n\n<mt:if name=\"error\">\n        <mtapp:statusmsg\n            id=\"error\"\n            class=\"error\">\n            <mt:var name=\"error\">\n        </mtapp:statusmsg>\n</mt:if>\n\n<mt:If name=\"not_unique_email\">\n        <mtapp:statusmsg\n            id=\"not-unique-email\"\n            class=\"alert\">\n            同じメールアドレスを持っているユーザーがいます。ユーザー名を入力してください。\n        </mtapp:statusmsg>\n</mt:if>\n\n<mt:if name=\"recovered\">\n<p>「<mt:var name=\"email\" escape=\"html\">」にパスワードをリセットするためのリンクを含むメールを送信しました。</p>\n\n<div class=\"actions-bar\">\n    <mt:If name=\"return_to\">\n        <a\n            href=\"<$mt:var name=\"return_to\" escape=\"html\" $>\"\n            accesskey=\"x\"\n            title=\"戻る (x)\"\n            class=\"action button primary-button\"\n            >戻る</a>\n        <mt:ElseIf name=\"can_signin\">\n        <a\n            href=\"<$mt:var name=\"script_url\"$>\"\n            accesskey=\"s\"\n            title=\"Movable Type にサインイン (s)\"\n            class=\"action button primary-button\"\n            >Movable Type にサインイン</a>\n        </a>\n    </mt:If>\n</div>\n\n\n<mt:else>\n    <form method=\"post\" action=\"<$mt:Var name=\"script_url\"$>\">\n    <input type=\"hidden\" name=\"__mode\" value=\"recover\" />\n    <input type=\"hidden\" name=\"return_to\" value=\"<mt:Var name=\"return_to\" escape=\"html\">\" />\n<fieldset>\n\n<mtapp:setting\n    id=\"email\"\n    label=\"電子メール\"\n    label_class=\"top-label\">\n    <input type=\"text\" name=\"email\" id=\"email\" class=\"ti\" value=\"<mt:var name=\"email\" escape=\"html\">\" />\n</mtapp:setting>\n\n<mt:If name=\"not_unique_email\">\n<mtapp:setting\n    id=\"name\"\n    label=\"ユーザー名\"\n    label_class=\"top-label\">\n    <input type=\"text\" name=\"name\" id=\"name\" class=\"ti\" value=\"<mt:var name=\"name\" escape=\"html\">\" />\n</mtapp:setting>\n</mt:If>\n\n<div class=\"actions-bar\">\n    <button\n        type=\"submit\"\n        accesskey=\"s\"\n        title=\"再設定 (s)\"\n        class=\"recover action primary-button\"\n        >再設定</button>\n    <button\n        onclick=\"history.back(); return false;\"\n        type=\"submit\"\n        accesskey=\"x\"\n        title=\"戻る (x)\"\n        class=\"action\"\n        >戻る</button>\n</div>\n\n</fieldset>\n</form>\n</mt:if>\n\n<$mt:Include module=\"シンプルフッター\"$>\n\n','new_password_reset_form',0);
INSERT INTO `mt_template` VALUES (46,0,0,0,1,1,'2012-02-08 05:45:55','register_form',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','登録フォーム',NULL,1,'\n<$mt:Var name=\"body_class\" value=\"mt-profile-register\"$>\n<mt:SetVarBlock name=\"page_title\">サインアップ</mt:SetVarBlock>\n<mt:SetVarBlock name=\"title\"><mt:If name=\"blog_id\"><$mt:BlogName encode_html=\"1\"$>: </mt:If><$mt:Var name=\"page_title\"$></mt:SetVarBlock>\n\n<$mt:Include module=\"シンプルヘッダー\"$>\n\n<mt:If name=\"error\">\n    <mt:App:StatusMsg\n        id=\"generic-error\"\n        class=\"error\">\n        <$mt:Var name=\"error\"$>\n    </mt:App:StatusMsg>\n</mt:If>\n<mt:If name=\"message\">\n    <mt:App:StatusMsg\n        id=\"message\"\n        class=\"alert\">\n        <$mt:Var name=\"message\"$>\n    </mt:App:StatusMsg>\n</mt:If>\n\n<mt:SetVarBlock name=\"common_fields\" id=\"common_fields\">\n    <mt:App:Setting\n        id=\"nickname\"\n        label=\"表示名\"\n        label_class=\"top-label\"\n        required=\"1\"\n        hint=\"あなたのコメントに表示される名前です。\">\n        <input type=\"text\" name=\"nickname\" id=\"nickname\" class=\"ti\" value=\"<$mt:Var name=\"nickname\" escape=\"html\"$>\" />\n    </mt:App:Setting>\n\n    <mt:App:Setting\n        id=\"email\"\n        label=\"電子メール\"\n        label_class=\"top-label\"\n        required=\"1\"\n        hint=\"あなたのメールアドレスです。\">\n        <input type=\"text\" name=\"email\" id=\"email\" class=\"ti\" value=\"<$mt:Var name=\"email\" escape=\"html\"$>\" />\n    </mt:App:Setting>\n</mt:SetVarBlock>\n\n<form method=\"post\" action=\"<$mt:Var name=\"script_url\"$>\">\n<input type=\"hidden\" name=\"blog_id\" value=\"<$mt:Var name=\"blog_id\" escape=\"html\"$>\" />\n<input type=\"hidden\" name=\"return_to\" value=\"<$mt:Var name=\"return_to\" escape=\"html\"$>\" />\n<mt:If name=\"auth_mode_mt\">\n<input type=\"hidden\" name=\"__mode\" value=\"do_register\" />\n\n    <mt:App:Setting\n        id=\"username\"\n        label=\"ユーザー名\"\n        label_class=\"top-label\"\n        required=\"1\"\n        hint=\"あなたのログイン名です。\">\n        <input type=\"text\" name=\"username\" id=\"username\" class=\"ti\" value=\"<$mt:Var name=\"username\" escape=\"html\"$>\" />\n    </mt:App:Setting>\n\n    <$mt:Var name=\"common_fields\"$>\n\n    <mt:App:Setting\n        id=\"password\"\n        label=\"初期パスワード\"\n        label_class=\"top-label\"\n        required=\"1\"\n        hint=\"パスワード選択してください。\">\n        <input type=\"password\" name=\"password\" id=\"password\" class=\"pw\" value=\"\" />\n    </mt:App:Setting>\n\n    <mt:App:Setting\n        id=\"pass_verify\"\n        label=\"パスワード再入力\"\n        label_class=\"top-label\"\n        required=\"1\"\n        hint=\"確認のため、パスワードを再入力してください。\">\n        <input type=\"password\" name=\"pass_verify\" id=\"pass_verify\" class=\"pw\" value=\"\" />\n    </mt:App:Setting>\n\n    <mt:App:Setting\n        id=\"url\"\n        label=\"ウェブサイトURL\"\n        label_class=\"top-label\"\n        hint=\"あなたのウェブサイトのURLです。(オプション)\">\n        <input type=\"text\" name=\"url\" id=\"url\" class=\"ti\" value=\"<$mt:Var name=\"url\" escape=\"html\"$>\" />\n    </mt:App:Setting>\n\n<mt:If name=\"captcha_fields\"><div class=\"setting\">\n<$mt:Var name=\"captcha_fields\"$>\n</div></mt:If>\n\n<mt:Else>\n<input type=\"hidden\" name=\"__mode\" value=\"do_register\" />\n<input type=\"hidden\" name=\"external_auth\" value=\"1\" />\n    <mt:App:Setting\n        id=\"external_username\"\n        label=\"ユーザー名\"\n        required=\"1\"\n        hint=\"あなたのログイン名です。\">\n        <input type=\"text\" name=\"username\" id=\"username\" class=\"ti\" value=\"<$mt:Var name=\"username\" escape=\"html\"$>\" />\n    </mt:App:Setting>\n</mt:If>\n\n<mt:Loop name=\"field_loop\">\n    <mt:If name=\"__first__\">\n    <input type=\"hidden\" name=\"_type\" value=\"author\" id=\"obj_type\" />\n    <input type=\"hidden\" name=\"customfield_beacon\" value=\"1\" id=\"customfield_beacon\" />\n    </mt:If>\n    <!-- start-customfield_<$mt:Var name=\"basename\"$> -->\n    <mt:App:Setting\n    id=\"$field_id\"\n    label=\"$name\"\n    hint=\"$description\"\n    shown=\"$show_field\"\n    show_hint=\"$show_hint\"\n    required=\"$required\">\n\n    <$mt:Var name=\"field_html\"$>\n\n    </mt:App:Setting>\n    <!-- end-customfield_<$mt:Var name=\"basename\"$> -->\n</mt:Loop>\n\n    <div class=\"actions-bar\">\n        <button\n            type=\"submit\"\n            class=\"sign-up action primary-button\"\n            accesskey=\"s\"\n            >サインアップ</button>\n    </div>\n\n</form>\n\n<$mt:Include module=\"シンプルフッター\"$>\n\n','register_form',0);
INSERT INTO `mt_template` VALUES (47,0,0,0,1,1,'2012-02-08 05:45:55','register_confirmation',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','登録の確認',NULL,1,'\n<$mt:Var name=\"body_class\" value=\"mt-profile-register\"$>\n<mt:If name=\"resend\">\n    <mt:SetVarBlock name=\"page_title\">確認メール送信完了</mt:SetVarBlock>\n<mt:Else>\n    <mt:SetVarBlock name=\"page_title\">プロフィールを作成しました。</mt:SetVarBlock>\n</mt:If>\n<mt:SetVarBlock name=\"title\"><mt:If name=\"blog_id\"><$mt:BlogName encode_html=\"1\"$>: </mt:If><$mt:Var name=\"page_title\"$></mt:SetVarBlock>\n\n<$mt:Include module=\"シンプルヘッダー\"$>\n\n<p>登録を完了するためにまずアカウントを確認する必要があります。<$mt:Var name=\"email\" escape=\"html\"$>にメールを送信しました。</p>\n\n<p>メールを確認して、メールに書かれたリンクをクリックすることで、アカウントを確認して有効化できます。</p>\n\n<mt:if name=\"return_to\">\n<p><a href=\"<$mt:Var name=\"return_to\"$>\">元のページに戻る</a></p>\n</mt:if>\n\n<$mt:Include module=\"シンプルフッター\"$>\n\n','register_confirmation',0);
INSERT INTO `mt_template` VALUES (48,0,0,0,1,1,'2012-02-08 05:45:55','navigation',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','ナビゲーション',NULL,1,'\n<div id=\"header-menu\">\n    <ul>\n<mt:IfBlog>\n        <li><a href=\"<$mt:Link template=\"main_index\"$>\">ホーム</a></li>\n        <li><a href=\"<$mt:Link template=\"archive_index\"$>\">アーカイブ</a></li>\n<mt:Else>\n        <li><a href=\"<$mt:Var name=\"blog_url\"$>\">ホーム</a></li>\n</mt:IfBlog>\n    </ul>\n</div>\n\n','custom',0);
INSERT INTO `mt_template` VALUES (49,0,0,0,1,1,'2012-02-08 05:45:55','footer-email',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','メールフッター',NULL,1,'\n-- \nPowered by Movable Type <$mt:Version$>\n<$mt:Var name=\"portal_url\"$>\n','custom',0);
INSERT INTO `mt_template` VALUES (50,0,0,0,1,1,'2012-02-08 05:45:55','commenter_notify',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','コメントの通知',NULL,1,'これは新しいユーザーがブログ「<$mt:BlogName$>」に登録を完了したことを通知するメールです。新しいユーザーの情報は以下に記載されています。\n\n新規登録ユーザー:\nユーザー名: <$mt:CommenterUsername$> \n名前: <$mt:CommenterName$>\nメール: <$mt:CommenterEmail$>\n\nこのユーザーの情報を見たり編集する場合には、下記のURLをクリックするか、URLをコピーしてブラウザのアドレス欄に貼り付けてください。\n<$mt:Var name=\"profile_url\"$>\n\n<$mt:Include module=\"メールフッター\"$>\n','email',0);
INSERT INTO `mt_template` VALUES (51,0,0,0,1,1,'2012-02-08 05:45:55','signin',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','サインイン',NULL,1,'\n<div class=\"widget-sign-in widget\">\n    <h3 class=\"widget-header\">サインイン</h3>\n    <div id=\"signin-widget-content\" class=\"widget-content\"></div>\n</div>\n<script type=\"text/javascript\">\n/* <![CDATA[ */\nfunction mtUpdateSignInWidget(u) {\n    var separator = \'&nbsp;&nbsp;|&nbsp;&nbsp;\';\n    var el = document.getElementById(\'signin-widget-content\');\n    var content = \'\';\n    if (!el) return;\n    if (u) {\n        if (u && u.is_authenticated) {\n            user = u;\n            mtSaveUser();\n        } else {\n            // user really isn\'t logged in; so let\'s do this!\n            return mtSignIn();\n        }\n    } else {\n        u = mtGetUser();\n    }\n    if (u && u.name) {\n        var url;\n        if (u.is_authenticated) {\n            url = \'<$mt:CGIPath$><$mt:CommunityScript$>?__mode=view&blog_id=<$mt:BlogID$>\';\n            url += \'&static=\' + encodeURIComponent( location.href );\n        } else if (u.url) {\n            url = u.url;\n        } else {\n            url = null;\n        }\n        if (url)\n            content += \'<a href=\"\' + url + \'\">\' + u.name + \'</a>\';\n        else\n            content += \'\' + u.name + \'\';\n        if (u.is_author)\n            content += separator + \'<a href=\"<$mt:CGIPath$><$mt:CommunityScript$>?__mode=edit&blog_id=<$mt:BlogID$>&return_to=\' + encodeURIComponent(document.URL) + \'\">ユーザー情報の編集</a>\';\n        <mt:Unless name=\"hide_signout_link\">\n        content += separator + \'<a href=\"javascript:void(0)\" onclick=\"return mtSignOutOnClick()\">サインアウト</a>\';\n        </mt:Unless>\n    } else if (u && u.is_banned) {\n        content = \'このブログにサインインする権限がありません。\';\n    } else {\n        content = \'<a href=\"javascript:void(0)\" onclick=\"return mtSignInOnClick(\\\'signin-widget-content\\\')\">サインイン</a>\';\n        <mt:IfExternalUserManagement>\n        <mt:Else>\n        <mt:IfCommenterRegistrationAllowed>\n        content += separator + \'<a href=\"<$mt:CGIPath$><$mt:CommunityScript$>?__mode=register&blog_id=<$mt:BlogID$>&return_to=\' + encodeURIComponent(document.URL) + \'\">登録</a>\';\n        </mt:IfCommenterRegistrationAllowed>\n        </mt:IfExternalUserManagement>\n    }\n    el.innerHTML = content;\n}\nmtAttachEvent(\'usersignin\', mtUpdateSignInWidget);\nmtUpdateSignInWidget();\n/* ]]> */\n</script>\n\n','custom',0);
INSERT INTO `mt_template` VALUES (52,0,0,0,1,1,'2012-02-08 05:45:55','new_password',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','新しいパスワードの設定フォーム',NULL,1,'\n<mt:var name=\"screen_id\" value=\"change-password\">\n<mt:var name=\"body_class\" value=\"mt-profile-reset\">\n<mt:setvarblock name=\"page_title\">パスワードの変更</mt:setvarblock>\n<mt:setvar name=\"complete\" value=\"1\">\n<mt:SetVarBlock name=\"title\"><mt:If name=\"blog_id\"><$mt:BlogName encode_html=\"1\"$>: </mt:If><$mt:Var name=\"page_title\"$></mt:SetVarBlock>\n\n<$mt:Include module=\"シンプルヘッダー\"$>\n\n<mt:if name=\"error\">\n        <mtapp:statusmsg\n            id=\"error\"\n            class=\"error\">\n            <mt:var name=\"error\">\n        </mtapp:statusmsg>\n</mt:if>\n\n    <form method=\"post\" action=\"<mt:var name=\"script_url\">\">\n    <input type=\"hidden\" name=\"__mode\" value=\"new_pw\" />\n    <input type=\"hidden\" name=\"token\" value=\"<mt:var name=\"token\" escape=\"html\">\" />\n    <input type=\"hidden\" name=\"email\" value=\"<mt:var name=\"email\" escape=\"html\">\" />\n<fieldset>\n\n<mtapp:setting\n    id=\"name\"\n    label=\"新しいパスワード\"\n    label_class=\"top-label\">\n    <input type=\"password\" name=\"password\" id=\"password\" value=\"<mt:var name=\"password\">\" class=\"ti\" />\n</mtapp:setting>\n\n<mtapp:setting\n    id=\"name\"\n    label=\"新しいパスワード確認\"\n    label_class=\"top-label\">\n    <input type=\"password\" name=\"password_again\" id=\"password_again\" value=\"<mt:var name=\"password_again\">\" class=\"ti\" />\n</mtapp:setting>\n\n<div class=\"actions-bar\">\n    <button\n        type=\"submit\"\n        accesskey=\"s\"\n        title=\"変更\"\n        class=\"action primary-button\"\n        >変更</button>\n</div>\n\n</fieldset>\n</form>\n<$mt:Include module=\"シンプルフッター\"$>\n\n\n','new_password',0);
INSERT INTO `mt_template` VALUES (53,0,0,0,1,1,'2012-02-08 05:45:55','login_form',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','ログインフォーム',NULL,1,'<$mt:Var name=\"screen_id\" value=\"sign-in\"$>\n<$mt:Var name=\"body_class\" value=\"mt-profile-login\"$>\n<mt:SetVarBlock name=\"page_title\">サインイン</mt:SetVarBlock>\n<mt:SetVarBlock name=\"title\"><mt:If name=\"blog_id\"><$mt:BlogName encode_html=\"1\"$>: </mt:If><$mt:Var name=\"page_title\"$></mt:SetVarBlock>\n\n\n\n<$mt:Include module=\"シンプルヘッダー\"$>\n\n<mt:If name=\"auth_loop\">\n<mtapp:setting\n    id=\"comment_signin_options\"\n    label=\"サインイン...\">\n    <select name=\"auth_options\" id=\"auth_options\">\n        <option value=\"<mt:var name=\"default_id\" escape=\"html\">\"><mt:var name=\"default_signin\" escape=\"html\"></option>\n    <mt:loop name=\"auth_loop\">\n        <option value=\"<mt:var name=\"id\" escape=\"html\">\"><mt:var name=\"name\" escape=\"html\"></option>\n    </mt:loop>\n    </select>\n</mtapp:setting>\n</mt:If>\n\n<mt:If name=\"message\">\n    <mtapp:statusmsg\n        id=\"message\"\n        class=\"alert\">\n        <$mt:Var name=\"message\"$>\n    </mt:App:StatusMsg>\n</mt:If>\n<mt:If name=\"error\">\n    <mtapp:statusmsg\n        id=\"generic-error\"\n        class=\"error\">\n        <$mt:Var name=\"error\"$>\n    </mt:App:StatusMsg>\n</mt:If>\n\n<div id=\"signin_with_<mt:var name=\"default_id\" escape=\"html\">\"<mt:Unless name=\"enabled_MovableType\"> style=\"display: none\"</mt:Unless>>\n    <form method=\"post\" action=\"<$mt:Var name=\"script_url\"$>\">\n        <input type=\"hidden\" name=\"__mode\" value=\"do_login\" />\n        <input type=\"hidden\" name=\"blog_id\" value=\"<$mt:Var name=\"blog_id\"$>\" />\n        <input type=\"hidden\" name=\"return_to\" value=\"<$mt:Var name=\"return_to\"$>\" />\n    <mt:Unless name=\"auth_mode_mt\">\n        <input type=\"hidden\" name=\"external_auth\" value=\"1\" />\n    </mt:Unless>\n\n        <mtapp:setting\n            id=\"username\"\n            label=\"ユーザー名\"\n            label_class=\"top-label\">\n            <input type=\"text\" name=\"username\" id=\"username\" class=\"ti\" />\n        </mt:App:Setting>\n\n        <mtapp:setting\n            id=\"password\"\n            label=\"パスワード\"\n            label_class=\"top-label\">\n            <input type=\"password\" name=\"password\" id=\"password\" class=\"pw\" />\n        </mt:App:Setting>\n\n        <p><label>ログイン情報を記憶する</label> <input type=\"checkbox\" name=\"remember\" value=\"1\" accesskey=\"r\" /></p>\n\n        <div class=\"actions-bar actions-bar-buttom\">\n            <button\n                type=\"submit\"\n                class=\"sign-in action primary-button\"\n                accesskey=\"s\"\n                >サインイン</button>\n    <mt:If name=\"can_recover_password\">\n                <p id=\"forget-password\"><a href=\"<$mt:Var name=\"script_url\"$>?__mode=start_recover&amp;return_to=<$mt:Var name=\"return_to\" escape=\"url\"$>\">パスワードをお忘れですか?</a></p>\n    </mt:If>\n       </div>\n    <mt:IfCommenterRegistrationAllowed>\n        <mt:IfExternalUserManagement>\n        <mt:else>\n            <mt:If name=\"blog_id\">\n       <p id=\"sign-up\">アカウントがないときは<a href=\"<$mt:Var name=\"script_url\"$>?__mode=register&blog_id=<$mt:Var name=\"blog_id\"$>&return_to=<$mt:Var name=\"return_to\" escape=\"url\"$>\">サインアップ</a>してください。</p>\n            </mt:If>\n        </mt:IfExternalUserManagement>\n    </mt:IfCommenterRegistrationAllowed>\n\n   </form>\n</div>\n\n<mt:If name=\"auth_loop\">\n<dl>\n<mt:Loop name=\"auth_loop\">\n    <dd>\n        <div id=\"signin_with_<mt:var name=\"id\" escape=\"html\">\"<mt:If name=\"name\" ne=\"$default_signin\"> style=\"display: none\"</mt:If>>\n            <$mt:Var name=\"login_form\"$>\n        </div>\n    </dd>\n</mt:Loop>\n</dl>\n</mt:If>\n\n<script type=\"text/javascript\">\n/* <![CDATA[ */\njQuery(function() {\n    jQuery(\'#auth_options\').change(function() {\n        jQuery(\'div[id^=signin_with_]\').hide();\n        jQuery(\'#signin_with_\'+jQuery(this).val()).show();\n    });\n});\n/* ]]> */\n</script>\n<$mt:Include module=\"シンプルフッター\"$>\n\n','login_form',0);
INSERT INTO `mt_template` VALUES (54,0,0,0,1,1,'2012-02-08 05:45:55','verify-subscribe',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','購読の確認',NULL,1,'<$mt:BlogName$>のアップデート通知にご登録いただきありがとうございました。以下のリンクから登録を完了させてください。\n\n<$mt:Var name=\"script_path\"$>?__mode=confirm;email=<$mt:Var name=\"email\" escape=\"url\"$>;magic=<$mt:Var name=\"magic\" escape=\"url\"$>;blog_id=<$mt:BlogID$><mt:If name=\"redirect_url\">;redirect=<$mt:Var name=\"redirect_url\" escape=\"url\"$><mt:Else><mt:If name=\"entry\">;entry_id=<$mt:EntryID$></mt:If></mt:If>\n\nリンクをクリックできない場合は、お使いのウェブブラウザに貼り付けてください。\n\n<$mt:Include module=\"メールフッター\"$>\n','email',0);
INSERT INTO `mt_template` VALUES (55,0,0,0,1,1,'2012-02-08 05:45:55','search',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','検索',NULL,1,'<div class=\"header-search\">\n    <form method=\"get\" action=\"<$mt:CGIPath$><$mt:SearchScript$>\">\n<mt:IfBlog>\n        <input type=\"hidden\" name=\"IncludeBlogs\" value=\"<$mt:BlogID$>\" />\n</mt:IfBlog>\n        <input type=\"hidden\" name=\"limit\" value=\"<$mt:SearchMaxResults$>\" />\n        <label for=\"search\">検索</label>\n        <input id=\"search\" class=\"query\" name=\"search\" value=\"<$mt:SearchString$>\" />\n        <input class=\"submit\" type=\"submit\" value=\"Go\" />\n    </form>\n</div>\n','custom',0);
INSERT INTO `mt_template` VALUES (56,0,0,0,1,1,'2012-02-08 05:45:55','profile_edit_form',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','プロフィールの編集フォーム',NULL,1,'\n<$mt:Var name=\"body_class\" value=\"mt-profile-edit\"$>\n<mt:SetVarBlock name=\"page_title\">ユーザー情報の編集</mt:SetVarBlock>\n<mt:SetVarBlock name=\"title\"><mt:If name=\"blog_id\"><$mt:BlogName encode_html=\"1\"$>: </mt:If><$mt:Var name=\"page_title\"$></mt:SetVarBlock>\n\n<mt:SetVarBlock name=\"content_header\" id=\"content_header\">\n<mt:If name=\"saved\">\n    <mtapp:statusmsg\n        id=\"saved\"\n        class=\"success\">\n        ユーザー情報を更新しました。<mt:If name=\"blog_id\"> <a href=\"<$mt:Var name=\"return_to\" escape=\"html\"$>\">元のページに戻る</a> / <a href=\"<$mt:CGIPath$><$mt:CommunityScript$>?__mode=view<mt:If name=\"blog_id\">&blog_id=<$mt:Var name=\"blog_id\"$></mt:If>\">プロフィールを表示する</a></mt:If>\n    </mt:App:StatusMsg>\n</mt:If>\n<mt:If name=\"error\">\n    <mtapp:statusmsg\n        id=\"generic-error\"\n        class=\"error\">\n        <$mt:Var name=\"error\"$>\n    </mt:App:StatusMsg>\n</mt:If>\n</mt:SetVarBlock>\n\n<$mt:Include module=\"シンプルヘッダー\"$>\n\n<$mt:Var name=\"content_header\"$>\n\n<form name=\"profile\" id=\"profile\" method=\"post\" action=\"<$mt:Var name=\"script_url\"$>\" enctype=\"multipart/form-data\">\n    <input type=\"hidden\" name=\"__mode\" value=\"save\" />\n    <mt:If name=\"blog_id\">\n    <input type=\"hidden\" name=\"blog_id\" value=\"<$mt:Var name=\"blog_id\" escape=\"html\"$>\" />\n    </mt:If>\n    <input type=\"hidden\" name=\"magic_token\" value=\"<$mt:Var name=\"magic_token\"$>\" />\n    <input type=\"hidden\" name=\"return_args\" value=\"<$mt:Var name=\"return_args\" escape=\"html\"$>\" />\n    <input type=\"hidden\" name=\"return_to\" value=\"<$mt:Var name=\"return_to\" escape=\"html\"$>\" />\n<mt:If name=\"id\">\n    <input type=\"hidden\" name=\"id\" value=\"<$mt:Var name=\"id\" escape=\"html\"$>\" />\n</mt:If>\n\n<mt:SetVarBlock name=\"field-content\"><span id=\"name\"><$mt:Var name=\"name\" escape=\"html\"$></span><input type=\"hidden\" name=\"name\" class=\"ti\" value=\"<$mt:Var name=\"name\" escape=\"html\"$>\" /></mt:SetVarBlock>\n<$mt:Include module=\"フォームフィールド\" id=\"name\" class=\"\" required=\"1\" label=\"ユーザー名\"$>\n\n<mt:SetVarBlock name=\"field-content\"><input type=\"text\" name=\"nickname\" id=\"nickname\" class=\"ti\" value=\"<$mt:Var name=\"nickname\" escape=\"html\"$>\" /></mt:SetVarBlock>\n<$mt:Include module=\"フォームフィールド\" id=\"nickname\" class=\"\" required=\"$auth_type_mt\" label=\"表示名\"$>\n\n<mt:SetVarBlock name=\"field-content\"><input type=\"text\" name=\"email\" id=\"email\" class=\"ti\" value=\"<$mt:Var name=\"email\" escape=\"html\"$>\" /></mt:SetVarBlock>\n<$mt:Include module=\"フォームフィールド\" id=\"email\" class=\"\" required=\"$auth_type_mt\" label=\"電子メール\"$>\n\n<mt:If name=\"auth_mode_mt\">\n\n    <mt:If name=\"can_modify_password\">\n    <mt:SetVarBlock name=\"field-content\"><input type=\"password\" name=\"pass\" id=\"pass\" class=\"pw\" /></mt:SetVarBlock>\n    <$mt:Include module=\"フォームフィールド\" id=\"pass\" class=\"\" label=\"新しいパスワード\"$>\n\n    <mt:SetVarBlock name=\"field-content\"><input type=\"password\" name=\"pass_verify\" id=\"pass_verify\" class=\"pw\" /></mt:SetVarBlock>\n    <$mt:Include module=\"フォームフィールド\" id=\"pass_verify\" class=\"\" label=\"パスワード確認\"$>\n    </mt:If>\n\n<mt:Else>\n    <input type=\"hidden\" name=\"external_auth\" value=\"1\" />\n</mt:If>\n\n<mt:SetVarBlock name=\"field-content\"><input type=\"text\" name=\"url\" id=\"url\" class=\"ti\" value=\"<$mt:Var name=\"url\" escape=\"html\"$>\"  /></mt:SetVarBlock>\n<$mt:Include module=\"フォームフィールド\" id=\"url\" class=\"\" label=\"ウェブサイトURL\"$>\n\n<mt:SetVarBlock name=\"field-content\">\n    <mt:If name=\"userpic\">\n        <$mt:Var name=\"userpic\"$>\n        <div>\n        <input type=\"file\" name=\"file\" id=\"file\" class=\"fi\" />\n        </div>\n    <mt:else>\n        <div>\n        <input type=\"file\" name=\"file\" id=\"file\" class=\"fi\" />\n        </div>\n    </mt:If>\n</mt:SetVarBlock>\n<$mt:Include module=\"フォームフィールド\" id=\"file\" class=\"\" label=\"プロフィール画像\"$>\n\n<mt:Loop name=\"field_loop\">\n    <mt:If name=\"__first__\">\n    <input type=\"hidden\" name=\"_type\" value=\"author\" id=\"obj_type\" />\n    <input type=\"hidden\" name=\"customfield_beacon\" value=\"1\" id=\"customfield_beacon\" />\n    </mt:If>\n    <!-- start-customfield_<$mt:Var name=\"basename\"$> -->\n    <mtapp:setting\n        id=\"$field_id\"\n        label=\"$name\"\n        hint=\"$description\"\n        shown=\"$show_field\"\n        show_hint=\"$show_hint\"\n        required=\"$required\"\n        content_class=\"$content_class\">\n\n    <$mt:Var name=\"field_html\"$>\n\n    </mtapp:setting>\n    <!-- end-customfield_<$mt:Var name=\"basename\"$> -->\n</mt:Loop>\n\n<div class=\"actions-bar actions-bar-login\">\n    <button\n        type=\"submit\"\n        accesskey=\"s\"\n        title=\"保存\"\n        class=\"save action primary-button\"\n        >保存</button>\n</div>\n\n</form>\n\n<mt:If name=\"blog_id\">\n<p><a href=\"<$mt:Var name=\"return_to\" escape=\"html\"$>\">元のページに戻る</a> / <a href=\"<$mt:CGIPath$><$mt:CommunityScript$>?__mode=view<mt:If name=\"blog_id\">&blog_id=<$mt:Var name=\"blog_id\"$></mt:If>\">プロフィールを表示する</a></p>\n</mt:If>\n\n<$mt:Include module=\"シンプルフッター\"$>\n\n','profile_edit_form',0);
INSERT INTO `mt_template` VALUES (57,0,0,0,1,1,'2012-02-08 05:45:55','profile_feed',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','プロフィールフィード',NULL,1,'<?xml version=\"1.0\" encoding=\"<$mt:PublishCharset$>\"?>\n<feed xmlns=\"http://www.w3.org/2005/Atom\">\n    <title><$mt:Var name=\"feed_title\"$></title>\n    <generator uri=\"http://www.sixapart.com/movabletype/\"><$mt:ProductName version=\"1\"$></generator>\n    <link rel=\"self\" href=\"<$mt:Var name=\"feed_self\"$>\" />\n    <link type=\"text/html\" rel=\"alternate\" href=\"<$mt:CGIPath$><$mt:CommunityScript$>?__mode=view&amp;id=<$mt:AuthorID$><mt:If name=\"blog_id\">&amp;blog_id=<$mt:Var name=\"blog_id\"$></mt:If>\" title=\"<$mt:AuthorDisplayName encode_html=\"1\"$>\"/>\n    <mt:If name=\"feed_updated\"><updated><$mt:Var name=\"feed_updated\"$></updated></mt:If>\n    <id><$mt:Var name=\"feed_self\"$></id>\n<mt:If name=\"entry\">\n    <mt:Entries lastn=\"15\">\n    <entry>\n        <title><$mt:EntryTitle remove_html=\"1\" encode_xml=\"1\"$></title>\n        <link rel=\"alternate\" type=\"text/html\" href=\"<$mt:EntryPermalink encode_xml=\"1\"$>\" />\n        <id><$mt:EntryAtomID$></id>\n\n        <published><$mt:EntryDate utc=\"1\" format=\"%Y-%m-%dT%H:%M:%SZ\"$></published>\n        <updated><$mt:EntryModifiedDate utc=\"1\" format=\"%Y-%m-%dT%H:%M:%SZ\"$></updated>\n\n        <summary><$mt:EntryExcerpt remove_html=\"1\" encode_xml=\"1\"$></summary>\n        <author>\n            <name><$mt:EntryAuthorDisplayName encode_xml=\"1\"$></name>\n            <uri><$mt:CGIPath encode_xml=\"1\"$><$mt:CommunityScript encode_xml=\"1\"$>?__mode=view&amp;blog_id=<$mt:BlogID$>&amp;id=<$mt:EntryAuthorID$></uri>\n        </author>\n        <mt:EntryCategories>\n            <category term=\"<$mt:CategoryLabel encode_xml=\"1\"$>\" scheme=\"http://www.sixapart.com/ns/types#category\" />\n        </mt:EntryCategories>\n        <mt:EntryTags><category term=\"<$mt:TagName normalize=\"1\" encode_xml=\"1\"$>\" label=\"<$mt:TagName encode_xml=\"1\"$>\" scheme=\"http://www.sixapart.com/ns/types#tag\" /></mt:EntryTags>\n        <content type=\"html\" xml:lang=\"<$mt:BlogLanguage ietf=\"1\"$>\" xml:base=\"<$mt:BlogURL encode_xml=\"1\"$>\">\n            <$mt:EntryBody encode_xml=\"1\"$>\n            <$mt:EntryMore encode_xml=\"1\"$>\n        </content>\n    </entry>\n    </mt:Entries>\n<mt:ElseIf name=\"comment\">\n    <mt:Comments lastn=\"15\">\n    <entry>\n        <title><$mt:CommentBody trim=\"20\" remove_html=\"1\" encode_xml=\"1\"$></title>\n        <link rel=\"alternate\" type=\"text/html\" href=\"<$mt:CommentLink encode_xml=\"1\"$>\" />\n        <id><mt:CommentEntry><$mt:EntryAtomID$></mt:CommentEntry>#<$mt:CommentID$></id>\n        <updated><$mt:CommentDate utc=\"1\" format=\"%Y-%m-%dT%H:%M:%SZ\"$></updated>\n        <author>\n            <name><$mt:CommentAuthor encode_xml=\"1\"$></name>\n            <mt:If tag=\"CommenterURL\"><uri><$mt:CommenterURL encode_xml=\"1\"$></uri></mt:If>\n        </author>\n        <content type=\"html\" xml:lang=\"<$mt:BlogLanguage ietf=\"1\"$>\" xml:base=\"<$mt:BlogURL encode_xml=\"1\"$>\">\n            <$mt:CommentBody encode_xml=\"1\"$>\n        </content>\n    </entry>\n    </mt:Comments>\n<mt:ElseIf name=\"action\">\n    <mt:Actions>\n    <entry>\n        <mt:ActionsEntry>\n        <title><$mt:EntryBlogName remove_html=\"1\" encode_xml=\"1\"$>に<$mt:EntryTitle remove_html=\"1\" encode_xml=\"1\"$>を作成しました。</title>\n        <link rel=\"alternate\" type=\"text/html\" href=\"<$mt:EntryPermalink encode_xml=\"1\"$>\" />\n        <id><$mt:EntryAtomID$></id>\n        <published><$mt:EntryDate utc=\"1\" format=\"%Y-%m-%dT%H:%M:%SZ\"$></published>\n        <updated><$mt:EntryModifiedDate utc=\"1\" format=\"%Y-%m-%dT%H:%M:%SZ\"$></updated>\n        <summary><$mt:EntryExcerpt remove_html=\"1\" encode_xml=\"1\"$></summary>\n        <author>\n            <name><$mt:EntryAuthorDisplayName encode_xml=\"1\"$></name>\n            <uri><$mt:CGIPath encode_xml=\"1\"$><$mt:CommunityScript encode_xml=\"1\"$>?__mode=view&amp;blog_id=<$mt:BlogID$>&amp;id=<$mt:EntryAuthorID$></uri>\n        </author>\n        <mt:EntryCategories><category term=\"<$mt:CategoryLabel encode_xml=\"1\"$>\" scheme=\"http://www.sixapart.com/ns/types#category\" /></mt:EntryCategories>\n        <mt:EntryTags><category term=\"<$mt:TagName normalize=\"1\" encode_xml=\"1\"$>\" label=\"<$mt:TagName encode_xml=\"1\"$>\" scheme=\"http://www.sixapart.com/ns/types#tag\" /></mt:EntryTags>\n        <content type=\"html\" xml:lang=\"<$mt:BlogLanguage ietf=\"1\"$>\" xml:base=\"<$mt:BlogURL encode_xml=\"1\"$>\">\n            <$mt:EntryBody encode_xml=\"1\"$>\n            <$mt:EntryMore encode_xml=\"1\"$>\n        </content>\n        </mt:ActionsEntry>\n        <mt:ActionsComment>\n        <title><mt:CommentEntry><$mt:EntryTitle remove_html=\"1\" encode_xml=\"1\"$>(<$mt:EntryBlogName remove_html=\"1\" encode_xml=\"1\"$></mt:CommentEntry>)へコメントしました。</title>\n        <link rel=\"alternate\" type=\"text/html\" href=\"<$mt:CommentLink encode_xml=\"1\"$>\" />\n        <id><mt:CommentEntry><$mt:EntryAtomID$></mt:CommentEntry>#<$mt:CommentID$></id>\n        <updated><$mt:CommentDate utc=\"1\" format=\"%Y-%m-%dT%H:%M:%SZ\"$></updated>\n        <author>\n            <name><$mt:CommentAuthor encode_xml=\"1\"$></name>\n            <mt:If tag=\"CommenterURL\"><uri><$mt:CommenterURL encode_xml=\"1\"$></uri></mt:If>\n        </author>\n        <content type=\"html\" xml:lang=\"<$mt:BlogLanguage ietf=\"1\"$>\" xml:base=\"<$mt:BlogURL encode_xml=\"1\"$>\">\n            <$mt:CommentBody encode_xml=\"1\"$>\n        </content>\n        </mt:ActionsComment>\n        <mt:ActionsFavorite>\n        <title><$mt:EntryTitle remove_html=\"1\" encode_xml=\"1\"$>(<$mt:EntryBlogName remove_html=\"1\" encode_xml=\"1\"$>)をお気に入りに追加しました。</title>\n        <link rel=\"alternate\" type=\"text/html\" href=\"<$mt:EntryPermalink encode_xml=\"1\"$>\" />\n        <id><$mt:EntryAtomID$></id>\n        <published><$mt:EntryDate utc=\"1\" format=\"%Y-%m-%dT%H:%M:%SZ\"$></published>\n        <updated><$mt:EntryModifiedDate utc=\"1\" format=\"%Y-%m-%dT%H:%M:%SZ\"$></updated>\n        <author>\n            <name><$mt:EntryAuthorDisplayName encode_xml=\"1\"$></name>\n            <uri><$mt:CGIPath encode_xml=\"1\"$><$mt:CommunityScript encode_xml=\"1\"$>?__mode=view&amp;blog_id=<$mt:BlogID$>&amp;id=<$mt:EntryAuthorID$></uri>\n        </author>\n        <content type=\"html\" xml:lang=\"<$mt:BlogLanguage ietf=\"1\"$>\" xml:base=\"<$mt:BlogURL encode_xml=\"1\"$>\">\n            <![CDATA[<p><$mt:EntryAuthorDisplayName remove_html=\"1\" encode_xml=\"1\"$>が<a href=\"<$mt:EntryPermalink encode_xml=\"1\"$>\"><$mt:EntryTitle remove_html=\"1\" encode_xml=\"1\"$></a>(<$mt:EntryBlogName remove_html=\"1\" encode_xml=\"1\"$>)をお気に入りに追加しました。</p>]]>\n        </content>\n        </mt:ActionsFavorite>\n    </entry>\n    </mt:Actions>\n</mt:If>\n</feed>\n','profile_feed',0);
INSERT INTO `mt_template` VALUES (58,0,0,0,1,1,'2012-02-08 05:45:55','footer',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','フッター',NULL,1,'                        </div>\n                    </div>\n<mt:Unless name=\"hide_sidebar\">\n                    <$mt:Include module=\"サイドバー\"$>\n</mt:Unless>\n                </div>\n            </div>\n            <div id=\"footer\">\n                <div id=\"footer-inner\">\n                    <div id=\"footer-content\">\n                        <div class=\"widget-powered widget\">\n                            <div class=\"widget-content\">\n                                <span>Powered by <a href=\"http://www.sixapart.jp/movabletype/\"><$MTProductName$></a></span>\n                            </div>\n                        </div>\n<mt:BlogIfCCLicense>\n                        <div class=\"widget-creative-commons widget\">\n                            <div class=\"widget-content\">\n                                このブログは<a href=\"<$mt:BlogCCLicenseURL$>\">クリエイティブ・コモンズ</a>でライセンスされています。\n                            </div>\n                        </div>\n</mt:BlogIfCCLicense>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n    <$mt:Var name=\"html_body_footer\"$>\n</body>\n</html>\n','custom',0);
INSERT INTO `mt_template` VALUES (59,0,0,0,1,1,'2012-02-08 05:45:55','commenter_confirm',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','コメントの確認',NULL,1,'<$mt:BlogName$>にコメントするために登録していただきありがとうございます。 \n\nセキュリティ上の理由から、登録を完了する前にアカウントとメールアドレスの確認を行っています。確認を完了次第、<$mt:BlogName$>にコメントできるようになります。\n\nアカウントの確認のため、次のURLをクリックするか、コピーしてブラウザのアドレス欄に貼り付けてください。\n\n<$mt:Var name=\"confirm_url\"$>\n\nこのメールに覚えがない場合や、<$mt:BlogName$>に登録するのをやめたい場合は、何もする必要はありません。\n\nご協力ありがとうございます。\n\n \n<mt:If tag=\"AuthorDisplayName\"><$mt:AuthorDisplayName encode_html=\"1\"$><mt:Else>Movable Type</mt:If>\n\n<$mt:Include module=\"メールフッター\"$>\n','email',0);
INSERT INTO `mt_template` VALUES (60,0,0,0,1,1,'2012-02-08 05:45:55','userpic',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','プロフィール画像',NULL,1,'<div class=\"user-pic\">\n<mt:If name=\"userpic_type\" eq=\"profile\">\n    <img src=\"<mt:If tag=\"AuthorUserpicURL\"><$mt:AuthorUserpicURL$><mt:Else><$mt:StaticWebPath$>images/default-userpic-90.jpg</mt:If>\"\n        width=\"<$mt:Var name=\"userpic_size\"$>\" height=\"<$mt:Var name=\"userpic_size\"$>\" alt=\"<$mt:AuthorDisplayName encode_html=\"1\"$>\" />\n    <mt:If tag=\"AuthorAuthType\" ne=\"MT\"><mt:If tag=\"AuthorAuthIconURL\"><span class=\"auth-icon\"><img src=\"<$mt:AuthorAuthIconURL size=\"logo_small\"$>\" alt=\"\"/></span></mt:If></mt:If>\n<mt:Else name=\"userpic_type\" eq=\"commenter\">\n    <mt:IfCommenterIsAuthor><mt:Ignore><!-- If native MTCS user --></mt:Ignore>\n    <a href=\"<$mt:CGIPath$><$mt:CommunityScript$>?__mode=view&amp;blog_id=<$mt:BlogID encode_html=\"1\"$>&amp;id=<$mt:CommenterID$>\"><img\n        src=\"<mt:If tag=\"CommenterUserpic\"><$mt:CommenterUserpicURL$><mt:Else><$mt:StaticWebPath$>images/default-userpic-90.jpg</mt:If>\"\n        width=\"<$mt:Var name=\"userpic_size\"$>\" height=\"<$mt:Var name=\"userpic_size\"$>\" alt=\"<$mt:CommenterName encode_html=\"1\"$>\" /></a>\n    <mt:Else>\n        <mt:If tag=\"CommenterID\"><mt:Ignore><!-- If authenticated user (OpenID, Vox, LJ, etc) --></mt:Ignore>\n    <a href=\"<$mt:CGIPath$><$mt:CommunityScript$>?__mode=view&amp;blog_id=<$mt:BlogID encode_html=\"1\"$>&amp;id=<$mt:CommenterID$>\">\n        <img src=\"<mt:If tag=\"CommenterUserpic\"><$mt:CommenterUserpicURL$><mt:Else><$mt:StaticWebPath$>images/default-userpic-90.jpg</mt:If>\" \n            width=\"<$mt:Var name=\"userpic_size\"$>\" height=\"<$mt:Var name=\"userpic_size\"$>\" alt=\"<$mt:CommenterName encode_html=\"1\"$>\" />\n        <mt:If tag=\"CommenterAuthIconURL\"><span class=\"auth-icon\"><img src=\"<$mt:CommenterAuthIconURL size=\"logo_small\"$>\" alt=\"\"/></span></mt:If>\n    </a>\n        <mt:Else><mt:Ignore><!-- Else anonymous commenter --></mt:Ignore>\n    <a href=\"<$mt:CommentURL$>\"><img src=\"<mt:If tag=\"CommenterUserpic\"><$mt:CommenterUserpicURL$><mt:Else><$mt:StaticWebPath$>images/default-userpic-90.jpg</mt:If>\" \n        width=\"<$mt:Var name=\"userpic_size\"$>\" height=\"<$mt:Var name=\"userpic_size\"$>\" alt=\"\" /></a>\n        </mt:If>\n    </mt:IfCommenterIsAuthor>\n<mt:Else name=\"userpic_type\" eq=\"userlist\">\n    <a href=\"<$mt:CGIPath$><$mt:CommunityScript$>?__mode=view&amp;blog_id=<$mt:BlogID encode_html=\"1\"$>&amp;id=<$mt:AuthorID$>\"><img\n        src=\"<mt:If tag=\"AuthorUserpicURL\"><$mt:AuthorUserpicURL$><mt:Else><$mt:StaticWebPath$>images/default-userpic-90.jpg</mt:If>\"\n        width=\"<$mt:Var name=\"userpic_size\"$>\" height=\"<$mt:Var name=\"userpic_size\"$>\" alt=\"<$mt:AuthorDisplayName encode_html=\"1\"$>\" /></a>\n<mt:Else>\n    <a href=\"<$mt:CGIPath$><$mt:CommunityScript$>?__mode=view&amp;blog_id=<$mt:BlogID encode_html=\"1\"$>&amp;id=<$mt:EntryAuthorID$>\"><img\n        src=\"<mt:If tag=\"EntryAuthorUserpicURL\"><$mt:EntryAuthorUserpicURL$><mt:Else><$mt:StaticWebPath$>images/default-userpic-90.jpg</mt:If>\"\n        width=\"<$mt:Var name=\"userpic_size\"$>\" height=\"<$mt:Var name=\"userpic_size\"$>\" alt=\"<$mt:EntryAuthorDisplayName encode_html=\"1\"$>\" /></a>\n</mt:If>\n</div>\n','custom',0);
INSERT INTO `mt_template` VALUES (61,0,0,0,1,1,'2012-02-08 05:45:55','header',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','ヘッダー',NULL,1,'<mt:SetVarBlock name=\"html_head\" prepend=\"1\">\n\n    <mt:IfBlog>\n        <mt:Ignore>\n    <!-- Community Solution Blog variables -->\n    <!-- The below variables are used when there a blog is in context.  -->\n        </mt:Ignore>\n        <mt:SetVarBlock name=\"blog_id\"><$mt:BlogID$></mt:SetVarBlock>\n        <mt:SetVarBlock name=\"blog_name\"><$mt:BlogName encode_html=\"1\"$></mt:SetVarBlock>\n        <mt:SetVarBlock name=\"blog_description\"><$mt:BlogDescription$></mt:SetVarBlock>\n        <mt:SetVarBlock name=\"blog_url\"><$mt:BlogURL$></mt:SetVarBlock>\n        <mt:SetVarBlock name=\"profile_view_url\"><$mt:CGIPath$><$mt:CommunityScript$>?__mode=view&amp;blog_id=<$mt:BlogID encode_html=\"1\"$>&amp;id=</mt:SetVarBlock>\n        <link rel=\"stylesheet\" href=\"<$mt:Link template=\"styles\"$>\" type=\"text/css\" />\n        <link rel=\"alternate\" type=\"application/atom+xml\" title=\"Atom\" href=\"<$mt:Link template=\"feed_recent\"$>\" />\n        <link rel=\"start\" href=\"<$mt:BlogURL$>\" title=\"Home\" />\n        <script type=\"text/javascript\" src=\"<$mt:Link template=\"javascript\"$>\"></script>\n        <$mt:CCLicenseRDF$>\n    <mt:Else>\n        <mt:Ignore>\n    <!-- Community Solution Static variables -->\n    <!-- The below variables are used when there is no blog in context. Set them to defaults in the case that they are used w/o a blog context -->\n        </mt:Ignore>\n        <mt:SetVarBlock name=\"blog_name\">ブログ名</mt:SetVarBlock>\n        <mt:SetVarBlock name=\"blog_description\">ブログの説明</mt:SetVarBlock>\n        <mt:SetVarBlock name=\"blog_url\">#</mt:SetVarBlock>\n        <mt:SetVarBlock name=\"profile_view_url\"><$mt:CGIPath$><$mt:CommunityScript$>?__mode=view&amp;id=</mt:SetVarBlock>\n        <link rel=\"stylesheet\" href=\"<$mt:StaticWebPath$>themes-base/blog.css\" type=\"text/css\" />\n        <link rel=\"stylesheet\" href=\"<$mt:StaticWebPath$>themes-base/forum.css\" type=\"text/css\" />\n        <link rel=\"stylesheet\" href=\"<$mt:StaticWebPath$>themes/tristan-blue-4.2/screen.css\" type=\"text/css\" />\n        <script type=\"text/javascript\" charset=\"utf-8\"><$mt:Include module=\"GlobalJavaScript\"$></script>\n        <!-- END Community Solution Styles -->\n    </mt:IfBlog>\n\n</mt:SetVarBlock>\n\n<mt:If tag=\"BlogTemplateSetID\" eq=\"mt-community-forum\"><$mt:Var name=\"page_layout\" value=\"layout-wm\"$></mt:If>\n\n<!DOCTYPE html PUBLIC \"-//W3C//DTD XHTML 1.0 Transitional//EN\"\n    \"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd\">\n<html xmlns=\"http://www.w3.org/1999/xhtml\" xml:lang=\"<$mt:BlogLanguage$>\" lang=\"<$mt:BlogLanguage$>\" id=\"sixapart-standard\">\n<head>\n    <meta http-equiv=\"Content-Type\" content=\"text/html; charset=<$mt:PublishCharset$>\" />\n    <meta http-equiv=\"Content-Style-Type\" content=\"text/css\" />\n    <meta http-equiv=\"Content-Script-Type\" content=\"text/javascript\" />\n    <meta http-equiv=\"imagetoolbar\" content=\"no\" />    \n    <title><$mt:Var name=\"title\"$></title>\n    <meta name=\"generator\" content=\"<$mt:ProductName version=\"1\"$>\" />\n    <$mt:Var name=\"html_head\"$>\n    <mt:Ignore>\n    <!-- Community Solution Scoring -->\n    <!-- This updates any scoring controls on the page for the user who is signed-in. -->\n    </mt:Ignore>\n    <script type=\"text/javascript\">\n    /* <![CDATA[ */\n    mtAttachEvent(\'load\', mtUpdateScores);\n    mtAttachEvent(\'usersignin\', mtUpdateScores);\n    /* ]]> */\n    </script>\n</head>\n<body id=\"<mt:BlogTemplateSetID>\" class=\"<$mt:Var name=\"body_class\"$> <$mt:Var name=\"page_layout\"$>\">\n    <div id=\"container\">\n        <div id=\"container-inner\">\n            <div id=\"header\">\n                <div id=\"header-inner\">\n                    <div id=\"header-content\">\n<mt:If name=\"main_index\">\n                        <h1 id=\"header-name\"><a href=\"<$mt:Var name=\"blog_url\"$>\" accesskey=\"1\"><$mt:Var name=\"blog_name\"$></a></h1>\n    <mt:If name=\"blog_description\">\n                        <h2 id=\"header-description\"><$mt:Var name=\"blog_description\"$></h2>\n    </mt:If>\n<mt:Else>\n                        <div id=\"header-name\"><a href=\"<$mt:Var name=\"blog_url\"$>\" accesskey=\"1\"><$mt:Var name=\"blog_name\"$></a></div>\n    <mt:If name=\"blog_description\">\n                        <div id=\"header-description\"><$mt:Var name=\"blog_description\"$></div>\n    </mt:If>\n</mt:If>\n                        <$mt:Include module=\"ナビゲーション\"$>\n                        <$mt:Include module=\"サインイン\"$>\n                        <$mt:Include module=\"検索\"$>\n                   </div>\n                </div>\n            </div>\n            <div id=\"content\">\n                <div id=\"content-inner\">\n                    <div id=\"alpha\">\n                        <div id=\"alpha-inner\">\n\n','custom',0);
INSERT INTO `mt_template` VALUES (62,0,0,0,1,1,'2012-02-08 05:45:55','new-ping',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','新しいトラックバック',NULL,1,'<mt:If name=\"unapproved\"><mt:IfBlog><mt:if tag=\"EntryClass\" eq=\"entry\">ブログ \'<$mt:BlogName$>\' のブログ記事 \'<$mt:EntryTitle$>\' (ID:<$mt:EntryID$>) に未公開のトラックバックがあります。公開するまでこのトラックバックはブログに表示されません。<mt:Else>ブログ \'<$mt:BlogName$>\' のウェブページ \'<$mt:PageTitle$>\' (ID:<$mt:PageID$>) に未公開のトラックバックがあります。公開するまでこのトラックバックはブログに表示されません。</mt:If><mt:Else>ウェブサイト \'<$mt:WebsiteName$>\' のウェブページ \'<$mt:PageTitle$>\' (ID:<$mt:PageID$>) に未公開のトラックバックがあります。公開するまでこのトラックバックはウェブサイトに表示されません。</mt:IfBlog><mt:IfCategory>ブログ \'<$mt:BlogName$>\' のカテゴリ \'<$mt:CategoryLabel$>\' (ID:<$mt:CategoryID$>) に未公開のトラックバックがあります。公開するまでこのトラックバックはブログに表示されません。</mt:IfCategory></mt:If><mt:Unless name=\"unapproved\"><mt:IfBlog><mt:if tag=\"EntryClass\" eq=\"entry\">ブログ \'<$mt:BlogName$>\' のブログ記事 \'<$mt:EntryTitle$>\' (ID:<$mt:EntryID$>) に新しいトラックバックがあります。<mt:Else>ブログ \'<$mt:BlogName$>\' のウェブページ \'<$mt:PageTitle$>\' (ID:<$mt:PageID$>) に新しいトラックバックがあります。</mt:If><mt:Else>ウェブサイト \'<$mt:WebsiteName$>\' のウェブページ \'<$mt:PageTitle$>\' (ID:<$mt:PageID$>) に新しいトラックバックがあります。</mt:IfBlog><mt:IfCategory>ブログ \'<$mt:BlogName$>\' のカテゴリ \'<$mt:CategoryLabel$>\' (ID:<$mt:CategoryID$>) に新しいトラックバックがあります。</mt:IfCategory></mt:Unless>\n\n概要: \n<$mt:PingExcerpt wrap_text=\"72\"$>\n\nURL: <$mt:PingURL$>\nタイトル: <$mt:PingTitle$>\nブログ: <$mt:PingBlogName$>\nIPアドレス: <$mt:PingIP$>\n\n<mt:If name=\"state_editable\"><mt:If name=\"unapproved\">トラックバックを承認する: \n  <<$mt:Var name=\"approve_url\"$>>\n</mt:If></mt:If>\n<mt:If name=\"entry\"><mt:If tag=\"EntryPermalink\">トラックバックを見る: \n  <<$mt:EntryPermalink$>>\n</mt:If></mt:If>\n<mt:If name=\"state_editable\">トラックバックをスパムとして報告する: \n  <<$mt:Var name=\"spam_url\"$>>\n</mt:If>\nトラックバックの編集: \n  <<$mt:Var name=\"edit_url\"$>>\n\n<$mt:Include module=\"メールフッター\"$>\n','email',0);
INSERT INTO `mt_template` VALUES (63,0,0,0,1,1,'2012-02-08 05:45:55','new_entry_email',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','ブログ記事の投稿通知',NULL,1,'\nブログ「<$mt:BlogName$>」に新しいブログ記事「<$mt:EntryTitle$>」(ID: <$mt:EntryID$>)が投稿されました。\n\nユーザー: <$mt:EntryAuthor$> \nユーザーの表示名: <$mt:EntryAuthorDisplayName$> \n\nタイトル: <$mt:EntryTitle$>\n日付: <$mt:EntryDate mail=\"1\"$>\n\n<$mt:EntryExcerpt wrap_text=\"72\"$>\n\n\n表示する <<$mt:EntryPermalink$>>\n編集する <<$mt:Var name=\"edit_url\"$>>\n\n<$mt:Include module=\"メールフッター\"$>\n\n','email',0);
INSERT INTO `mt_template` VALUES (64,0,0,0,1,1,'2012-02-08 05:45:55','form_field',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','フォームフィールド',NULL,1,'<div id=\"<$mt:Var name=\"id\"$>-field\" class=\"field-top-label field pkg <$mt:Var name=\"class\"$>\">\n    <div class=\"field-inner\">\n        <div class=\"field-header\">\n            <label type=\"text\" id=\"<$mt:Var name=\"id\"$>-label\" for=\"<$mt:Var name=\"id\"$>\"><$mt:Var name=\"label\"$><mt:If name=\"required\"> *</mt:If></label>\n        </div>\n        <div class=\"field-content \">\n            <$mt:Var name=\"field-content\"$>\n        </div>\n    </div>\n</div>\n','custom',0);
INSERT INTO `mt_template` VALUES (65,0,0,0,1,1,'2012-02-08 05:45:55','new-comment',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','新しいコメント',NULL,1,'<mt:If name=\"unapproved\"><mt:IfBlog><mt:if tag=\"EntryClass\" eq=\"entry\">未公開のコメントがブログ \'<$mt:BlogName$>\' のブログ記事 \'<$mt:EntryTitle$>\' (ID:<$mt:EntryID$>) に投稿されました。公開するまでこのコメントはブログに表示されません。<mt:Else>未公開のコメントがブログ \'<$mt:BlogName$>\' のウェブページ \'<$mt:PageTitle$>\' (ID:<$mt:PageID$>) に投稿されました。公開するまでこのコメントはブログに表示されません。</mt:If><mt:Else>未公開のコメントがウェブサイト \'<$mt:WebsiteName$>\' のウェブページ \'<$mt:PageTitle$>\' (ID:<$mt:PageID$>) に投稿されました。公開するまでこのコメントはウェブサイトに表示されません。</mt:IfBlog></mt:If><mt:Unless name=\"unapproved\"><mt:IfBlog><mt:if tag=\"EntryClass\" eq=\"entry\">ブログ \'<$mt:BlogName$>\' のブログ記事 \'<$mt:EntryTitle$>\' (ID:<$mt:EntryID$>) に新しいコメントが投稿されました。<mt:Else>ブログ \'<$mt:BlogName$>\' のウェブページ \'<$mt:PageTitle$>\' (ID:<$mt:PageID$>) に新しいコメントが投稿されました。</mt:If><mt:Else>ウェブサイト \'<$mt:WebsiteName$>\' のウェブページ \'<$mt:PageTitle$>\' (ID:<$mt:PageID$>) に新しいコメントが投稿されました。</mt:IfBlog></mt:Unless>\n\n<$mt:CommentBody remove_html=\"1\" wrap_text=\"72\"$>\n\nコメント投稿者: <$mt:CommentAuthor$> \nメールアドレス: <$mt:CommentEmail$>\nURL: <$mt:CommentURL$>\nIPアドレス: <$mt:CommentIP$>\n\n<mt:If name=\"unapproved\"><mt:If name=\"state_editable\">コメントを承認する:\n  <<$mt:Var name=\"approve_url\"$>></mt:If></mt:If>\n<mt:If name=\"view_url\">コメントを見る:\n  <<$mt:Var name=\"view_url\"$>></mt:If>\nコメントを編集する:\n  <<$mt:Var name=\"edit_url\"$>>\n<mt:If name=\"state_editable\">コメントをスパムとして報告する:\n  <<$mt:Var name=\"spam_url\"$>></mt:If>\n\n<$mt:Include module=\"メールフッター\"$>\n','email',0);
INSERT INTO `mt_template` VALUES (66,0,0,0,1,1,'2012-02-08 05:45:55','simple_header',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','シンプルヘッダー',NULL,1,'<!DOCTYPE html PUBLIC \"-//W3C//DTD XHTML 1.0 Transitional//EN\" \"DTD/xhtml1-transitional.dtd\">\n<html xmlns=\"http://www.w3.org/1999/xhtml\" id=\"sixapart-standard\" xmlns:fb=\"http://www.facebook.com/2008/fbml\">\n<head>\n    <meta http-equiv=\"Content-Type\" content=\"text/html; charset=UTF-8\" />\n    <title><$mt:Var name=\"title\" encode_html=\"1\"$></title>\n<mt:SetVarBlock name=\"html_head\" prepend=\"1\">\n<mt:If name=\"mt_debug\">\n    <link rel=\"stylesheet\" href=\"<$mt:Var name=\"static_uri\"$>css/reset.css?v=<$mt:Var name=\"mt_version\" escape=\"url\"$>\" type=\"text/css\" />\n    <link rel=\"stylesheet\" href=\"<$mt:Var name=\"static_uri\"$>css/utilities.css?v=<$mt:Var name=\"mt_version\" escape=\"url\"$>\" type=\"text/css\" />\n    <link rel=\"stylesheet\" href=\"<$mt:Var name=\"static_uri\"$>css/messaging.css?v=<$mt:Var name=\"mt_version\" escape=\"url\"$>\" type=\"text/css\" />\n    <link rel=\"stylesheet\" href=\"<$mt:Var name=\"static_uri\"$>css/chromeless.css?v=<$mt:Var name=\"mt_version\" escape=\"url\"$>\" type=\"text/css\" />\n<mt:Else>\n    <link rel=\"stylesheet\" href=\"<$mt:Var name=\"static_uri\"$>css/simple.css?v=<$mt:Var name=\"mt_version\" escape=\"url\"$>\" type=\"text/css\" />\n</mt:If>\n    <link rel=\"stylesheet\" href=\"<$mt:var name=\"static_uri\"$>addons/Commercial.pack/styles-customfields.css\" type=\"text/css\" media=\"screen\" title=\"CustomFields Stylesheet\" charset=\"utf-8\" />\n    <!--[if lte IE 7]>\n    <link rel=\"stylesheet\" href=\"<$mt:var name=\"static_uri\"$>css/hacks/ie.css?v=<mt:var name=\"mt_version_id\" escape=\"url\">\" type=\"text/css\" />\n    <![endif]-->\n    <!--[if lte IE 6]>\n    <link rel=\"stylesheet\" href=\"<$mt:var name=\"static_uri\"$>css/hacks/ie6.css?v=<mt:var name=\"mt_version_id\" escape=\"url\">\" type=\"text/css\" />\n    <![endif]-->\n<mt:If name=\"local_lang_id\">\n    <mt:If name=\"local_lang_id\" ne=\"en-us\">\n    <link rel=\"stylesheet\" href=\"<$mt:Var name=\"static_uri\"$>styles_<$mt:Var name=\"local_lang_id\"$>.css?v=<$mt:Var name=\"mt_version\" escape=\"url\"$>\" />\n    </mt:If>\n</mt:If>\n    <script type=\"text/javascript\" src=\"<$mt:Var name=\"static_uri\"$>js/common/Core.js?v=<$mt:Var name=\"mt_version\" escape=\"url\"$>\"></script>\n    <script type=\"text/javascript\" src=\"<$mt:Var name=\"static_uri\"$>js/tc.js?v=<$mt:Var name=\"mt_version\" escape=\"url\"$>\"></script>\n    <script type=\"text/javascript\" src=\"<$mt:Var name=\"static_uri\"$>jquery/jquery.js?v=<$mt:Var name=\"mt_version\" escape=\"url\"$>\"></script>\n</mt:SetVarBlock>\n<$mt:Var name=\"html_head\"$>\n<script type=\"text/javascript\" src=\"<$mt:Var name=\"static_uri\"$>mt.js?v=<$mt:Var name=\"mt_version\" escape=\"url\"$>\"></script>\n<script language=\"javascript\" type=\"text/javascript\">\n<!--\nfunction showByID(id) {\n    var el = getByID(id);\n    if (el) el.style.display = \"block\";\n}\nfunction hideByID(id) {\n    var el = getByID(id);\n    if (el) el.style.display = \"none\";\n}\n// -->\n</script>\n<$mt:Var name=\"js_include\"$>\n</head>\n\n<body id=\"<$mt:Var name=\"screen_id\"$>\" class=\"chromeless dialog<mt:If name=\"mt_debug\"> debug</mt:If><mt:If name=\"error_page\"><mt:If name=\"is_full_screen\"> error-screen</mt:If> chromeless-wide</mt:If><mt:If name=\"screen_type\" eq=\"popup-screen\"> chromeless-wide</mt:If> <$mt:Var name=\"body_class\" default=\"dialog\"$>\">\n    <div id=\"container\">\n        <div id=\"brand\"><img alt=\"Movable Type\" src=\"<mt:var name=\"static_uri\">images/chromeless/mt_logo.png\" /></div>\n        <div id=\"nav\">\n<mt:If name=\"wizard_steps\">\n    <mt:Unless name=\"complete\">\n        <mt:If name=\"step\" ne=\"pre_start\">\n    <mt:Loop name=\"wizard_steps\">\n            <mt:Unless name=\"__last__\">\n        <div class=\"step<mt:If name=\"active\">_active</mt:If>\"></div>\n            </mt:Unless>\n    </mt:Loop>\n        </mt:If>\n    </mt:Unless>\n</mt:If>\n        </div>\n        <h1 id=\"page-title\" class=\"asset-name\"><$mt:Var name=\"page_title\"$></h1>\n','custom',0);
INSERT INTO `mt_template` VALUES (67,0,0,0,1,1,'2012-02-08 05:45:55','register_notification_email',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','登録通知',NULL,1,'これは新しいユーザーがブログ「<$mt:BlogName$>」に登録を完了したことを通知するメールです。新しいユーザーの情報は以下に記載されています。\n\n新規登録ユーザー:\nユーザー名: <$mt:CommenterUsername$> \n名前: <$mt:CommenterName$>\nメール: <$mt:CommenterEmail$>\n\nこのユーザーの情報を見たり編集する場合には、下記のURLをクリックするか、URLをコピーしてブラウザのアドレス欄に貼り付けてください。\n<$mt:Var name=\"profile_url\"$>\n\n<$mt:Include module=\"メールフッター\"$>\n\n','email',0);
INSERT INTO `mt_template` VALUES (68,0,0,0,1,1,'2012-02-08 05:45:55','comment_throttle',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','コメントスロットル',NULL,1,'<mt:SetVarBlock name=\"message\">これが間違いである場合は、Movable Typeにログインして、ブログの設定画面に進み、禁止IPリストからIPアドレスを削除してください。</mt:SetVarBlock>\n<$mt:BlogName$>を禁止しました。<$mt:Var name=\"throttle_seconds\"$>秒の間に許可された以上のコメントを送信してきました。\n\nこれは悪意のスクリプトがブログをコメントで飽和させるのを阻止するための措置です。以下のIPアドレスを禁止しました。\n\n<$mt:Var name=\"throttled_ip\"$>\n\n<$mt:Var name=\"message\"$>\n','email',0);
INSERT INTO `mt_template` VALUES (69,0,0,0,1,1,'2012-02-08 05:45:55','simple_footer',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','シンプルフッター',NULL,1,'\n</div><!--container-->\n<script type=\"text/javascript\">\n/* <![CDATA[ */\njQuery(\'input\').each(function() {\n    jQuery(this).addClass(jQuery(this).attr(\'type\'));\n});\n/* ]]> */\n</script>\n\n</body>\n</html>\n\n','custom',0);
INSERT INTO `mt_template` VALUES (70,2,0,0,1,1,'2012-02-08 05:46:18','entry_summary',NULL,NULL,NULL,NULL,'2012-02-08 05:46:18','ブログ記事の概要',NULL,1,'\n<$mt:EntryTrackbackData$>\n<div id=\"entry-<$mt:EntryID$>\" class=\"entry-asset asset hentry\">\n    <div class=\"asset-header\">\n        <h2 class=\"asset-name entry-title\"><a href=\"<$mt:EntryPermalink$>\" rel=\"bookmark\"><$mt:EntryTitle$></a></h2>\n        <$mt:Include module=\"ブログ記事のメタデータ\" blog_id=\"$website_id\"$>\n    </div>\n    <div class=\"asset-content entry-content\">\n<mt:If tag=\"EntryBody\">\n        <div class=\"asset-body\">\n            <$mt:EntryBody$>\n        </div>\n</mt:If>\n<mt:IfTagSearch>\n        <$mt:Include module=\"タグ\"$>\n</mt:IfTagSearch>\n<mt:If tag=\"EntryMore\" convert_breaks=\"0\">\n        <div class=\"asset-more-link\">\n            続きを読む: <a href=\"<$mt:EntryPermalink$>#more\" rel=\"bookmark\"><$mt:EntryTitle$></a>\n        </div>\n</mt:If>\n    </div>\n    <div class=\"asset-footer\"></div>\n</div>\n\n','custom',0);
INSERT INTO `mt_template` VALUES (71,2,0,0,1,1,'2012-02-08 05:46:18','trackbacks',NULL,NULL,NULL,NULL,'2012-02-08 05:46:18','トラックバック',NULL,1,'\n<mt:IfPingsActive>\n<div id=\"trackbacks\" class=\"trackbacks\">\n    <h2 class=\"trackbacks-header\"><$mt:EntryTrackbackCount singular=\"トラックバック(1)\" plural=\"トラックバック(#)\" none=\"トラックバック(0)\"$></h2>\n    <mt:IfPingsAccepted>\n    <div class=\"trackbacks-info\">\n        <p>トラックバックURL: <$mt:EntryTrackbackLink$></p>\n    </div>\n    </mt:IfPingsAccepted>\n    <mt:Pings>\n        <mt:PingsHeader>\n    <div class=\"trackbacks-content\">\n        </mt:PingsHeader>\n        <div class=\"trackback\" id=\"ping-<$mt:PingID$>\">\n            <div class=\"inner\">\n                <div class=\"trackback-header\">\n                    <div class=\"asset-meta\">\n                        <$mt:PingBlogName$> - <a href=\"<$mt:PingURL$>\"><$mt:PingTitle$></a> (<a href=\"#ping-<$mt:PingID$>\"><$mt:PingDate$></a>)\n                    </div>\n                </div>\n                <div class=\"trackback-content\">\n                    <p>\n                        <$mt:PingExcerpt$> <a href=\"<$mt:PingURL$>\">続きを読む</a>\n                    </p>\n                </div>\n            </div>\n        </div>\n        <mt:PingsFooter>\n    </div>\n        </mt:PingsFooter>\n    </mt:Pings>\n</div>\n</mt:IfPingsActive>\n\n\n','custom',0);
INSERT INTO `mt_template` VALUES (72,2,0,0,1,1,'2012-02-08 05:46:18','comment_preview',NULL,NULL,NULL,NULL,'2012-02-08 05:46:18','コメントプレビュー',NULL,1,'\n<$mt:Var name=\"page_layout\" value=\"layout-wm\"$>\n<mt:SetVarBlock name=\"page_title\">コメントのプレビュー</mt:SetVarBlock>\n<mt:SetVarBlock name=\"title\"><$mt:BlogName encode_html=\"1\"$>: <$mt:Var name=\"page_title\"$></mt:SetVarBlock>\n<mt:SetVarBlock name=\"html_head\">\n    <script type=\"text/javascript\">\n    /* <![CDATA[ */\n    var user = <$mt:UserSessionState$>;\n    var is_preview = true;\n    /* ]]> */\n    </script>\n</mt:SetVarBlock>\n<$mt:Include module=\"ヘッダー\" body_class=\"mt-comment-preview\"$>\n\n<h1 id=\"page-title\"><$mt:Var name=\"page_title\"$></h1>\n\n<$mt:Include module=\"コメント詳細\"$>\n\n<$mt:Include module=\"コメント入力フォーム\"$>\n\n<$mt:Include module=\"フッター\"$>\n\n\n','comment_preview',0);
INSERT INTO `mt_template` VALUES (73,2,0,0,1,1,'2012-02-08 05:46:18','powered_by_footer',NULL,NULL,NULL,NULL,'2012-02-08 05:46:18','Powered By (フッター)',NULL,1,'<div class=\"widget-powered widget\">\n    <div class=\"widget-content\">\n        <span>Powered by <a href=\"http://www.sixapart.jp/movabletype/\"><$MTProductName$></a></span>\n    </div>\n</div>\n','custom',0);
INSERT INTO `mt_template` VALUES (74,2,0,0,1,1,'2012-02-08 05:46:18','search_results',NULL,NULL,NULL,NULL,'2012-02-08 05:46:18','検索結果',NULL,1,'\n<mt:SetVarBlock name=\"page_title\">検索結果</mt:SetVarBlock>\n<mt:SetVarBlock name=\"title\"><$mt:BlogName encode_html=\"1\"$>: <$mt:Var name=\"page_title\"$></mt:SetVarBlock>\n\n<mt:Ignore><!-- Below Javascript adds ajax search capability --></mt:Ignore>\n<mt:SetVarBlock name=\"html_head\" append=\"1\">\n<script type=\"text/javascript\">\n/* <![CDATA[ */\nvar user = <$mt:UserSessionState$>;\n<mt:IfMoreResults>\nfunction getResults(page) {\n    page = parseInt(page);\n    if (timer) window.clearTimeout(timer);\n    var xh = mtGetXmlHttp();\n    if (!xh) return false;\n    var res = results[page];\n    if (!res) return;\n    var url = res[\'next_url\'];\n    if (!url) return;\n\n    xh.open(\'GET\', url + \'&format=js\', true);\n    xh.onreadystatechange = function() {\n        if ( xh.readyState == 4 ) {\n            if ( xh.status && ( xh.status != 200 ) ) {\n                // error - ignore\n            } else {\n                try {\n                    var page_results = eval(\"(\" + xh.responseText + \")\");\n                    if ( page_results[\'error\'] == null )\n                        results[page + 1] = page_results[\'result\'];\n                } catch (e) {\n                }\n            }\n        }\n    };\n    xh.send(null);\n}\n\nfunction swapContent(direction) {\n    if ( direction == undefined ) direction = 1;\n    var page_span = document.getElementById(\'current-page\');\n    if (!page_span) return true;\n    var next_page = direction + parseInt(page_span.innerHTML);\n    var res = results[next_page];\n    if (!res) return true;\n    var content = res[\'content\'];\n    if (!content) return true;\n    var div = document.getElementById(\'search-results\');\n    if (!div) return true;\n    div.innerHTML = content;\n    timer = window.setTimeout(\"getResults(\" + next_page + \")\", 1*1000);\n    window.scroll(0, 0);\n    return false;\n}\n<mt:Else><mt:IfPreviousResults>\nfunction swapContent(direction) {\n	return true;\n}</mt:IfPreviousResults>\n</mt:IfMoreResults>\n/* ]]> */\n</script>\n</mt:SetVarBlock>\n\n<$mt:Include module=\"ヘッダー\" body_class=\"mt-search-results\"$>\n\n<mt:SetVarTemplate id=\"search_results\" name=\"search_results\">  \n    <mt:SearchResults>\n        <mt:SearchResultsHeader>\n<div id=\"search-results\">\n    <span id=\"current-page\" class=\"hidden\"><$mt:CurrentPage$></span>\n    <h1 id=\"page-title\" class=\"search-results-header\">\n        <mt:IfStraightSearch>\n        「<$mt:SearchString$>」と一致するもの\n        </mt:IfStraightSearch>\n        <mt:IfTagSearch>\n        タグ「<$mt:SearchString$>」が付けられているもの\n        </mt:IfTagSearch>\n    </h1>\n    <div class=\"search-results-container autopagerize_page_element\">\n        </mt:SearchResultsHeader>\n    <$mt:Include module=\"ブログ記事の概要\" hide_counts=\"1\"$>\n        <mt:SearchResultsFooter>\n    </div><div class=\"autopagerize_insert_before\"></div>\n        <mt:Ignore>Used with the ajax search capability of the new search class</mt:Ignore>\n    <div class=\"content-nav\">\n        <mt:IfPreviousResults><a href=\"<$mt:PreviousLink$>\" rel=\"prev\" onclick=\"return swapContent(-1);\">&lt; 前</a>&nbsp;&nbsp;</mt:IfPreviousResults><mt:PagerBlock><mt:IfCurrentPage><$mt:Var name=\"__value__\"$><mt:Else><a href=\"<$mt:PagerLink$>\"><$mt:Var name=\"__value__\"$></a></mt:IfCurrentPage><mt:Unless name=\"__last__\">&nbsp;</mt:Unless></mt:PagerBlock><mt:IfMoreResults>&nbsp;&nbsp;<a href=\"<$mt:NextLink$>\" rel=\"next\" onclick=\"return swapContent();\">次 &gt;</a></mt:IfMoreResults>\n    </div>\n</div>\n        </mt:SearchResultsFooter>\n    </mt:SearchResults>\n</mt:SetVarTemplate>\n<$mt:Var name=\"search_results\"$>\n\n<mt:NoSearchResults>\n<h1 id=\"page-title\" class=\"search-results-header\">\n    <mt:IfStraightSearch>\n    「<$mt:SearchString$>」と一致するもの\n    </mt:IfStraightSearch>\n    <mt:IfTagSearch>\n    タグ「<$mt:SearchString$>」が付けられているもの\n    </mt:IfTagSearch>\n</h1>\n<p>「<$mt:SearchString$>」と一致する結果は見つかりませんでした。</p>\n</mt:NoSearchResults>\n\n<mt:NoSearch>\n<h1 id=\"page-title\" class=\"search-results-header\">例</h1>\n<p>すべての単語が順序に関係なく検索されます。フレーズで検索したいときは引用符で囲んでください。</p>\n<blockquote>\n    <p><code>\"movable type\"</code></p>\n</blockquote>\n<p>AND、OR、NOTを入れることで論理検索を行うこともできます。</p>\n<blockquote>\n    <p><code>個人 OR 出版</code></p>\n    <p><code>個人 NOT 出版</code></p>\n</blockquote>\n</mt:NoSearch>\n\n<mt:Ignore>Used with the ajax search capability of the new search class</mt:Ignore>\n<mt:IfMoreResults>\n<script type=\"text/javascript\">\n<!--\nvar div = document.getElementById(\'search-results\');\nvar results = {\n    \'<$mt:CurrentPage$>\': {\n        \'content\': div.innerHTML,\n        \'next_url\': \'<$mt:NextLink$>\'\n    }\n};\nvar timer = window.setTimeout(\"getResults(\" + <$mt:CurrentPage$> + \")\", 1*1000);\n//-->\n</script>\n</mt:IfMoreResults>\n<$mt:Include module=\"フッター\"$>\n\n','search_results',0);
