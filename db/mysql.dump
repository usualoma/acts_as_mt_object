INSERT INTO `mt_template` VALUES (1,1,0,0,1,1,'2012-02-08 05:45:55','trackbacks',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','トラックバック',NULL,1,'\n<mt:IfPingsActive>\n<div id=\"trackbacks\" class=\"trackbacks\">\n    <h2 class=\"trackbacks-header\"><$mt:EntryTrackbackCount singular=\"トラックバック(1)\" plural=\"トラックバック(#)\" none=\"トラックバック(0)\"$></h2>\n    <mt:IfPingsAccepted>\n    <div class=\"trackbacks-info\">\n        <p>トラックバックURL: <$mt:EntryTrackbackLink$></p>\n    </div>\n    </mt:IfPingsAccepted>\n    <mt:Pings>\n        <mt:PingsHeader>\n    <div class=\"trackbacks-content\">\n        </mt:PingsHeader>\n        <div class=\"trackback\" id=\"ping-<$mt:PingID$>\">\n            <div class=\"inner\">\n                <div class=\"trackback-header\">\n                    <div class=\"asset-meta\">\n                        <$mt:PingBlogName$> - <a href=\"<$mt:PingURL$>\"><$mt:PingTitle$></a> (<a href=\"#ping-<$mt:PingID$>\"><$mt:PingDate$></a>)\n                    </div>\n                </div>\n                <div class=\"trackback-content\">\n                    <p>\n                        <$mt:PingExcerpt$> <a href=\"<$mt:PingURL$>\">続きを読む</a>\n                    </p>\n                </div>\n            </div>\n        </div>\n        <mt:PingsFooter>\n    </div>\n        </mt:PingsFooter>\n    </mt:Pings>\n</div>\n</mt:IfPingsActive>\n\n\n','custom',0);
INSERT INTO `mt_template` VALUES (2,1,0,0,1,1,'2012-02-08 05:45:55','entry_summary',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','ブログ記事の概要',NULL,1,'\n<$mt:EntryTrackbackData$>\n<div id=\"entry-<$mt:EntryID$>\" class=\"entry-asset asset hentry\">\n    <div class=\"asset-header\">\n        <h2 class=\"asset-name entry-title\"><a href=\"<$mt:EntryPermalink$>\" rel=\"bookmark\"><$mt:EntryTitle$></a> - <a href=\"<$mt:EntryBlogURL$>\"><$mt:EntryBlogName$></a></h2>\n        <$mt:Include module=\"ブログ記事のメタデータ\" blog_id=\"$website_id\"$>\n    </div>\n    <div class=\"asset-content entry-content\">\n<mt:If tag=\"EntryBody\">\n        <div class=\"asset-body\">\n            <$mt:EntryBody$>\n        </div>\n</mt:If>\n<mt:IfTagSearch>\n        <$mt:Include module=\"タグ\"$>\n</mt:IfTagSearch>\n<mt:If tag=\"EntryMore\" convert_breaks=\"0\">\n        <div class=\"asset-more-link\">\n            続きを読む: <a href=\"<$mt:EntryPermalink$>#more\" rel=\"bookmark\"><$mt:EntryTitle$></a>\n        </div>\n</mt:If>\n    </div>\n    <div class=\"asset-footer\"></div>\n</div>\n\n','custom',0);
INSERT INTO `mt_template` VALUES (3,1,0,0,1,1,'2012-02-08 05:45:55','comment_preview',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','コメントプレビュー',NULL,1,'\n<$mt:Var name=\"page_layout\" value=\"layout-wm\"$>\n<mt:SetVarBlock name=\"page_title\">コメントのプレビュー</mt:SetVarBlock>\n<mt:SetVarBlock name=\"title\"><$mt:BlogName encode_html=\"1\"$>: <$mt:Var name=\"page_title\"$></mt:SetVarBlock>\n<mt:SetVarBlock name=\"html_head\">\n    <script type=\"text/javascript\">\n    /* <![CDATA[ */\n    var user = <$mt:UserSessionState$>;\n    var is_preview = true;\n    /* ]]> */\n    </script>\n</mt:SetVarBlock>\n<$mt:Include module=\"ヘッダー\" body_class=\"mt-comment-preview\"$>\n\n<h1 id=\"page-title\"><$mt:Var name=\"page_title\"$></h1>\n\n<$mt:Include module=\"コメント詳細\"$>\n\n<$mt:Include module=\"コメント入力フォーム\"$>\n\n<$mt:Include module=\"フッター\"$>\n\n\n','comment_preview',0);
INSERT INTO `mt_template` VALUES (4,1,0,0,1,1,'2012-02-08 05:45:55','powered_by_footer',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','Powered By (フッター)',NULL,1,'<div class=\"widget-powered widget\">\n    <div class=\"widget-content\">\n        <span>Powered by <a href=\"http://www.sixapart.jp/movabletype/\"><$MTProductName$></a></span>\n    </div>\n</div>\n','custom',0);
INSERT INTO `mt_template` VALUES (5,1,0,0,1,1,'2012-02-08 05:45:55','search_results',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','検索結果',NULL,1,'\n<$mt:Var name=\"page_layout\" value=\"layout-wm\"$>\n<mt:SetVarBlock name=\"page_title\">検索結果</mt:SetVarBlock>\n<mt:SetVarBlock name=\"title\"><$mt:BlogName encode_html=\"1\"$>: <$mt:Var name=\"page_title\"$></mt:SetVarBlock>\n\n<mt:Ignore><!-- Below Javascript adds ajax search capability --></mt:Ignore>\n<mt:SetVarBlock name=\"html_head\" append=\"1\">\n<script type=\"text/javascript\">\n/* <![CDATA[ */\nvar user = <$mt:UserSessionState$>;\n<mt:IfMoreResults>\nfunction getResults(page) {\n    page = parseInt(page);\n    if (timer) window.clearTimeout(timer);\n    var xh = mtGetXmlHttp();\n    if (!xh) return false;\n    var res = results[page];\n    if (!res) return;\n    var url = res[\'next_url\'];\n    if (!url) return;\n\n    xh.open(\'GET\', url + \'&format=js\', true);\n    xh.onreadystatechange = function() {\n        if ( xh.readyState == 4 ) {\n            if ( xh.status && ( xh.status != 200 ) ) {\n                // error - ignore\n            } else {\n                try {\n                    var page_results = eval(\"(\" + xh.responseText + \")\");\n                    if ( page_results[\'error\'] == null )\n                        results[page + 1] = page_results[\'result\'];\n                } catch (e) {\n                }\n            }\n        }\n    };\n    xh.send(null);\n}\n\nfunction swapContent(direction) {\n    if ( direction == undefined ) direction = 1;\n    var page_span = document.getElementById(\'current-page\');\n    if (!page_span) return true;\n    var next_page = direction + parseInt(page_span.innerHTML);\n    var res = results[next_page];\n    if (!res) return true;\n    var content = res[\'content\'];\n    if (!content) return true;\n    var div = document.getElementById(\'search-results\');\n    if (!div) return true;\n    div.innerHTML = content;\n    timer = window.setTimeout(\"getResults(\" + next_page + \")\", 1*1000);\n    window.scroll(0, 0);\n    return false;\n}\n<mt:Else><mt:IfPreviousResults>\nfunction swapContent(direction) {\n	return true;\n}</mt:IfPreviousResults>\n</mt:IfMoreResults>\n/* ]]> */\n</script>\n</mt:SetVarBlock>\n\n<$mt:Include module=\"ヘッダー\" body_class=\"mt-search-results\"$>\n\n<mt:SetVarTemplate id=\"search_results\" name=\"search_results\">  \n    <mt:SearchResults>\n        <mt:SearchResultsHeader>\n<div id=\"search-results\">\n    <span id=\"current-page\" class=\"hidden\"><$mt:CurrentPage$></span>\n    <h1 id=\"page-title\" class=\"search-results-header\">\n        <mt:IfStraightSearch>\n        「<$mt:SearchString$>」と一致するもの\n        </mt:IfStraightSearch>\n        <mt:IfTagSearch>\n        タグ「<$mt:SearchString$>」が付けられているもの\n        </mt:IfTagSearch>\n    </h1>\n    <div class=\"search-results-container autopagerize_page_element\">\n        </mt:SearchResultsHeader>\n    <$mt:Include module=\"ブログ記事の概要\" hide_counts=\"1\" local=\"1\"$>\n        <mt:SearchResultsFooter>\n    </div><div class=\"autopagerize_insert_before\"></div>\n        <mt:Ignore>Used with the ajax search capability of the new search class</mt:Ignore>\n    <div class=\"content-nav\">\n        <mt:IfPreviousResults><a href=\"<$mt:PreviousLink$>\" rel=\"prev\" onclick=\"return swapContent(-1);\">&lt; 前</a>&nbsp;&nbsp;</mt:IfPreviousResults><mt:PagerBlock><mt:IfCurrentPage><$mt:Var name=\"__value__\"$><mt:Else><a href=\"<$mt:PagerLink$>\"><$mt:Var name=\"__value__\"$></a></mt:IfCurrentPage><mt:Unless name=\"__last__\">&nbsp;</mt:Unless></mt:PagerBlock><mt:IfMoreResults>&nbsp;&nbsp;<a href=\"<$mt:NextLink$>\" rel=\"next\" onclick=\"return swapContent();\">次 &gt;</a></mt:IfMoreResults>\n    </div>\n</div>\n        </mt:SearchResultsFooter>\n    </mt:SearchResults>\n</mt:SetVarTemplate>\n<$mt:Var name=\"search_results\"$>\n\n<mt:NoSearchResults>\n<h1 id=\"page-title\" class=\"search-results-header\">\n    <mt:IfStraightSearch>\n    「<$mt:SearchString$>」と一致するもの\n    </mt:IfStraightSearch>\n    <mt:IfTagSearch>\n    タグ「<$mt:SearchString$>」が付けられているもの\n    </mt:IfTagSearch>\n</h1>\n<p>「<$mt:SearchString$>」と一致する結果は見つかりませんでした。</p>\n</mt:NoSearchResults>\n\n<mt:NoSearch>\n<h1 id=\"page-title\" class=\"search-results-header\">例</h1>\n<p>すべての単語が順序に関係なく検索されます。フレーズで検索したいときは引用符で囲んでください。</p>\n<blockquote>\n    <p><code>\"movable type\"</code></p>\n</blockquote>\n<p>AND、OR、NOTを入れることで論理検索を行うこともできます。</p>\n<blockquote>\n    <p><code>個人 OR 出版</code></p>\n    <p><code>個人 NOT 出版</code></p>\n</blockquote>\n</mt:NoSearch>\n\n<mt:Ignore>Used with the ajax search capability of the new search class</mt:Ignore>\n<mt:IfMoreResults>\n<script type=\"text/javascript\">\n<!--\nvar div = document.getElementById(\'search-results\');\nvar results = {\n    \'<$mt:CurrentPage$>\': {\n        \'content\': div.innerHTML,\n        \'next_url\': \'<$mt:NextLink$>\'\n    }\n};\nvar timer = window.setTimeout(\"getResults(\" + <$mt:CurrentPage$> + \")\", 1*1000);\n//-->\n</script>\n</mt:IfMoreResults>\n<$mt:Include module=\"フッター\"$>\n\n','search_results',0);
