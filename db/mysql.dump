INSERT INTO `mt_template` VALUES (5,1,0,0,1,1,'2012-02-08 05:45:55','search_results',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','Ê§úÁ¥¢ÁµêÊûú',NULL,1,'\n<$mt:Var name=\"page_layout\" value=\"layout-wm\"$>\n<mt:SetVarBlock name=\"page_title\">Ê§úÁ¥¢ÁµêÊûú</mt:SetVarBlock>\n<mt:SetVarBlock name=\"title\"><$mt:BlogName encode_html=\"1\"$>: <$mt:Var name=\"page_title\"$></mt:SetVarBlock>\n\n<mt:Ignore><!-- Below Javascript adds ajax search capability --></mt:Ignore>\n<mt:SetVarBlock name=\"html_head\" append=\"1\">\n<script type=\"text/javascript\">\n/* <![CDATA[ */\nvar user = <$mt:UserSessionState$>;\n<mt:IfMoreResults>\nfunction getResults(page) {\n    page = parseInt(page);\n    if (timer) window.clearTimeout(timer);\n    var xh = mtGetXmlHttp();\n    if (!xh) return false;\n    var res = results[page];\n    if (!res) return;\n    var url = res[\'next_url\'];\n    if (!url) return;\n\n    xh.open(\'GET\', url + \'&format=js\', true);\n    xh.onreadystatechange = function() {\n        if ( xh.readyState == 4 ) {\n            if ( xh.status && ( xh.status != 200 ) ) {\n                // error - ignore\n            } else {\n                try {\n                    var page_results = eval(\"(\" + xh.responseText + \")\");\n                    if ( page_results[\'error\'] == null )\n                        results[page + 1] = page_results[\'result\'];\n                } catch (e) {\n                }\n            }\n        }\n    };\n    xh.send(null);\n}\n\nfunction swapContent(direction) {\n    if ( direction == undefined ) direction = 1;\n    var page_span = document.getElementById(\'current-page\');\n    if (!page_span) return true;\n    var next_page = direction + parseInt(page_span.innerHTML);\n    var res = results[next_page];\n    if (!res) return true;\n    var content = res[\'content\'];\n    if (!content) return true;\n    var div = document.getElementById(\'search-results\');\n    if (!div) return true;\n    div.innerHTML = content;\n    timer = window.setTimeout(\"getResults(\" + next_page + \")\", 1*1000);\n    window.scroll(0, 0);\n    return false;\n}\n<mt:Else><mt:IfPreviousResults>\nfunction swapContent(direction) {\n	return true;\n}</mt:IfPreviousResults>\n</mt:IfMoreResults>\n/* ]]> */\n</script>\n</mt:SetVarBlock>\n\n<$mt:Include module=\"„Éò„ÉÉ„ÉÄ„Éº\" body_class=\"mt-search-results\"$>\n\n<mt:SetVarTemplate id=\"search_results\" name=\"search_results\">  \n    <mt:SearchResults>\n        <mt:SearchResultsHeader>\n<div id=\"search-results\">\n    <span id=\"current-page\" class=\"hidden\"><$mt:CurrentPage$></span>\n    <h1 id=\"page-title\" class=\"search-results-header\">\n        <mt:IfStraightSearch>\n        „Äå<$mt:SearchString$>„Äç„Å®‰∏ÄËá¥„Åô„Çã„ÇÇ„ÅÆ\n        </mt:IfStraightSearch>\n        <mt:IfTagSearch>\n        „Çø„Ç∞„Äå<$mt:SearchString$>„Äç„Åå‰ªò„Åë„Çâ„Çå„Å¶„ÅÑ„Çã„ÇÇ„ÅÆ\n        </mt:IfTagSearch>\n    </h1>\n    <div class=\"search-results-container autopagerize_page_element\">\n        </mt:SearchResultsHeader>\n    <$mt:Include module=\"„Éñ„É≠„Ç∞Ë®ò‰∫ã„ÅÆÊ¶ÇË¶Å\" hide_counts=\"1\" local=\"1\"$>\n        <mt:SearchResultsFooter>\n    </div><div class=\"autopagerize_insert_before\"></div>\n        <mt:Ignore>Used with the ajax search capability of the new search class</mt:Ignore>\n    <div class=\"content-nav\">\n        <mt:IfPreviousResults><a href=\"<$mt:PreviousLink$>\" rel=\"prev\" onclick=\"return swapContent(-1);\">&lt; Ââç</a>&nbsp;&nbsp;</mt:IfPreviousResults><mt:PagerBlock><mt:IfCurrentPage><$mt:Var name=\"__value__\"$><mt:Else><a href=\"<$mt:PagerLink$>\"><$mt:Var name=\"__value__\"$></a></mt:IfCurrentPage><mt:Unless name=\"__last__\">&nbsp;</mt:Unless></mt:PagerBlock><mt:IfMoreResults>&nbsp;&nbsp;<a href=\"<$mt:NextLink$>\" rel=\"next\" onclick=\"return swapContent();\">Ê¨° &gt;</a></mt:IfMoreResults>\n    </div>\n</div>\n        </mt:SearchResultsFooter>\n    </mt:SearchResults>\n</mt:SetVarTemplate>\n<$mt:Var name=\"search_results\"$>\n\n<mt:NoSearchResults>\n<h1 id=\"page-title\" class=\"search-results-header\">\n    <mt:IfStraightSearch>\n    „Äå<$mt:SearchString$>„Äç„Å®‰∏ÄËá¥„Åô„Çã„ÇÇ„ÅÆ\n    </mt:IfStraightSearch>\n    <mt:IfTagSearch>\n    „Çø„Ç∞„Äå<$mt:SearchString$>„Äç„Åå‰ªò„Åë„Çâ„Çå„Å¶„ÅÑ„Çã„ÇÇ„ÅÆ\n    </mt:IfTagSearch>\n</h1>\n<p>„Äå<$mt:SearchString$>„Äç„Å®‰∏ÄËá¥„Åô„ÇãÁµêÊûú„ÅØË¶ã„Å§„Åã„Çä„Åæ„Åõ„Çì„Åß„Åó„Åü„ÄÇ</p>\n</mt:NoSearchResults>\n\n<mt:NoSearch>\n<h1 id=\"page-title\" class=\"search-results-header\">‰æã</h1>\n<p>„Åô„Åπ„Å¶„ÅÆÂçòË™û„ÅåÈ†ÜÂ∫è„Å´Èñ¢‰øÇ„Å™„ÅèÊ§úÁ¥¢„Åï„Çå„Åæ„Åô„ÄÇ„Éï„É¨„Éº„Ç∫„ÅßÊ§úÁ¥¢„Åó„Åü„ÅÑ„Å®„Åç„ÅØÂºïÁî®Á¨¶„ÅßÂõ≤„Çì„Åß„Åè„Å†„Åï„ÅÑ„ÄÇ</p>\n<blockquote>\n    <p><code>\"movable type\"</code></p>\n</blockquote>\n<p>AND„ÄÅOR„ÄÅNOT„ÇíÂÖ•„Çå„Çã„Åì„Å®„ÅßË´ñÁêÜÊ§úÁ¥¢„ÇíË°å„ÅÜ„Åì„Å®„ÇÇ„Åß„Åç„Åæ„Åô„ÄÇ</p>\n<blockquote>\n    <p><code>ÂÄã‰∫∫ OR Âá∫Áâà</code></p>\n    <p><code>ÂÄã‰∫∫ NOT Âá∫Áâà</code></p>\n</blockquote>\n</mt:NoSearch>\n\n<mt:Ignore>Used with the ajax search capability of the new search class</mt:Ignore>\n<mt:IfMoreResults>\n<script type=\"text/javascript\">\n<!--\nvar div = document.getElementById(\'search-results\');\nvar results = {\n    \'<$mt:CurrentPage$>\': {\n        \'content\': div.innerHTML,\n        \'next_url\': \'<$mt:NextLink$>\'\n    }\n};\nvar timer = window.setTimeout(\"getResults(\" + <$mt:CurrentPage$> + \")\", 1*1000);\n//-->\n</script>\n</mt:IfMoreResults>\n<$mt:Include module=\"„Éï„ÉÉ„Çø„Éº\"$>\n\n','search_results',0);
