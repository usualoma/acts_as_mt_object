INSERT INTO `mt_template` VALUES (38,0,0,0,1,1,'2012-02-08 05:45:55','profile_view',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','プロフィール',NULL,1,'\n<$mt:Var name=\"page_layout\" value=\"layout-wt\"$>\n<mt:IfBlog>\n    <mt:SetVarBlock name=\"blog_name\"><$mt:BlogName encode_html=\"1\"$></mt:SetVarBlock>\n    <mt:SetVarBlock name=\"page_title\">ユーザーのプロフィール</mt:SetVarBlock>\n    <mt:SetVarBlock name=\"title\"><mt:If name=\"blog_id\"><$mt:Var name=\"blog_name\"$>: </mt:If><$mt:Var name=\"page_title\"$>: <$mt:AuthorDisplayName encode_html=\"1\"$></mt:SetVarBlock>\n<mt:Else>\n    <mt:SetVarBlock name=\"blog_name\">ブログ名</mt:SetVarBlock>\n    <mt:SetVarBlock name=\"page_title\">ユーザーのプロフィール</mt:SetVarBlock>\n    <mt:SetVarBlock name=\"title\"><mt:If name=\"blog_id\"><$mt:Var name=\"blog_name\"$>: </mt:If><$mt:Var name=\"page_title\"$>: <$mt:AuthorDisplayName encode_html=\"1\"$></mt:SetVarBlock>\n</mt:IfBlog>\n<$mt:Var name=\"hide_signout_link\" value=\"1\"$>\n<mt:SetVarBlock name=\"html_head\">\n<link type=\"application/atom+xml\" rel=\"alternate\" title=\"最近の<$mt:AuthorDisplayName encode_html=\"1\"$>のアクション\" href=\"<$mt:CGIPath$><$mt:CommunityScript$>?__mode=feed&amp;_type=actions&amp;blog_id=<$mt:BlogID$>&amp;id=<$mt:AuthorID$>\" />\n<link type=\"application/atom+xml\" rel=\"alternate\" title=\"<$mt:AuthorDisplayName encode_html=\"1\"$>の最近のブログ記事\" href=\"<$mt:CGIPath$><$mt:CommunityScript$>?__mode=feed&amp;_type=posts&amp;blog_id=<$mt:BlogID$>&amp;id=<$mt:AuthorID$>\" />\n<link type=\"application/atom+xml\" rel=\"alternate\" title=\"<$mt:AuthorDisplayName encode_html=\"1\"$>からのコメント\" href=\"<$mt:CGIPath$><$mt:CommunityScript$>?__mode=feed&amp;_type=comments&amp;blog_id=<$mt:BlogID$>&amp;id=<$mt:AuthorID$>\" />\n<link type=\"application/atom+xml\" rel=\"alternate\" title=\"<$mt:AuthorDisplayName encode_html=\"1\"$>のコメントへの返信\" href=\"<$mt:CGIPath$><$mt:CommunityScript$>?__mode=feed&amp;_type=replies&amp;blog_id=<$mt:BlogID$>&amp;id=<$mt:AuthorID$>\" />\n<script type=\"text/javascript\">\n<!--\nfunction relations(following, followed, u) {\n    mtSetUser(u);\n    if ( u && u.name )\n        conditional_block(<mt:If name=\"profile_self\">true<mt:Else>false</mt:If>, \'edit_profile\');\n<mt:If name=\"profile_self\">\n    var a = document.getElementById(\'edit_profile_link\');\n    a.href = \'<$mt:CGIPath$><$mt:CommunityScript$>?__mode=edit&blog_id=<$mt:BlogID$>&return_to=\' + encodeURIComponent(document.URL);\n</mt:If>\n    conditional_block(followed, \'followed_<$mt:AuthorID$>\');\n    conditional_block(following, \'following_<$mt:AuthorID$>\');\n}\n//-->\n</script>\n</mt:SetVarBlock>\n\n<$mt:Include module=\"ヘッダー\" body_class=\"mt-profile-view\"$>\n\n<$mt:Include module=\"プロフィール画像\" userpic_type=\"profile\" userpic_size=\"90\"$>\n\n<h1 id=\"page-title\"><$mt:AuthorDisplayName encode_html=\"1\"$></h1>\n\n<mt:IfLoggedIn element_id=\"edit_profile\">\n<p><a id=\"edit_profile_link\" href=\"#\">ユーザー情報の編集</a></p>\n<mt:Else>\n<p class=\"edit-relation\">\n    <span id=\"following-status\"></span>\n    <mt:AuthorIfFollowing script=\"0\">\n    <$mt:AuthorDisplayName encode_html=\"1\"$>に注目しています。 <$mt:AuthorUnfollowLink text=\"注目をやめる\"$>\n    <mt:Else>\n    <$mt:AuthorFollowLink text=\"注目する\"$>\n    </mt:AuthorIfFollowing>\n&nbsp;</p>\n\n    <mt:Ignore>\n        <mt:AuthorIfFollowed script=\"0\">\n<p><$mt:AuthorDisplayName encode_html=\"1\"$>に注目されています。</p>\n        <mt:Else>\n<p><$mt:AuthorDisplayName encode_html=\"1\"$>は注目していません。</p>\n        </mt:AuthorIfFollowed>\n    </mt:Ignore>\n\n    <mt:SetVarBlock name=\"html_body_footer\" append=\"1\">\n    <script type=\"text/javascript\" src=\"<$mt:CGIPath$><$mt:CommunityScript$>?__mode=relations_js&author_id=<$mt:AuthorID$><mt:If name=\"blog_id\">&blog_id=<$mt:Var name=\"blog_id\"$></mt:If>\"></script>\n    </mt:SetVarBlock>\n</mt:IfLoggedIn>\n\n<ul id=\"profile-data\">\n<mt:If tag=\"AuthorURL\">\n    <li><strong>ウェブサイト</strong> <a href=\"<$mt:AuthorURL encode_html=\"1\"$>\" target=\"_blank\"><$mt:AuthorURL regex_replace=\"/^\\w+:\\/\\//\",\"\" encode_html=\"1\"$></a></li>\n</mt:If>\n<mt:Loop name=\"field_loop\">\n    <mt:If name=\"value\">\n    <li><strong><$mt:Var name=\"name\" escape=\"html\"$>:</strong><mt:If name=\"type\" eq=\"datetime\"><$mt:Var name=\"field_value\"$><mt:Else><$mt:Var name=\"field_value\" sanitize=\"1\"$></mt:If></li>\n    </mt:If>\n</mt:Loop>\n</ul>\n\n<ul id=\"tabs\"> \n    <li><a href=\"#profile-recent-actions\" class=\"active\" onclick=\"switchTabs(\'tabs\',this)\">最近のアクション</a></li> \n    <li><a href=\"#profile-comment-threads\" class=\"\" onclick=\"switchTabs(\'tabs\',this)\">コメントスレッド</a></li> \n</ul>\n\n<div id=\"profile-recent-actions\" class=\"tab_content\"> \n    <h3 class=\"tab_content_label\">最近のアクション</h3>\n\n<mt:Actions namespace=\"community_pack_recommend\" include_blogs=\"all\" sort_order=\"descend\" lastn=\"30\">\n    <mt:ActionsHeader>\n    <ul class=\"recent-actions\">\n    </mt:ActionsHeader>\n    <mt:ActionsEntry>\n        <li class=\"entry icon-entry\">\n            <a href=\"<$mt:EntryBlogURL$>\" class=\"icon-blog\"><$mt:EntryBlogName$></a>に<a href=\"<$mt:EntryLink$>\"><$mt:EntryTitle encode_html=\"1\"$></a>を作成しました。\n            <div class=\"excerpt\"><$mt:EntryExcerpt$></div>\n        </li>\n    </mt:ActionsEntry>\n    <mt:ActionsComment>\n        <li class=\"comment icon-comment\">\n            <mt:CommentEntry><a href=\"<$mt:CommentLink$>\"><$mt:EntryTitle encode_html=\"1\"$></a></mt:CommentEntry>にコメントしました。\n            <div class=\"excerpt\"><$mt:CommentBody words=\"40\"$>...</div>\n        </li>\n    </mt:ActionsComment>\n    <mt:ActionsFavorite>\n        <li class=\"score icon-score\">\n            <a href=\"<$mt:EntryBlogURL$>\" class=\"icon-blog\"><$mt:EntryBlogName$></a>の<a href=\"<$mt:EntryLink$>\"><$mt:EntryTitle encode_html=\"1\"$></a>をお気に入りに追加しました。\n        </li>\n    </mt:ActionsFavorite>\n    <mt:ActionsFooter>\n    </ul>\n    </mt:ActionsFooter>\n<mt:Else>\n    <p class=\"note\">最近アクションはありません</p>\n\n</mt:Actions>\n\n<img src=\"<$mt:StaticWebPath$>images/status_icons/feed.gif\" alt=\"購読する\" /> <a href=\"<$mt:CGIPath$><$mt:CommunityScript$>?__mode=feed&amp;_type=actions&amp;blog_id=<$mt:BlogID$>&amp;id=<$mt:AuthorID$>\">最近の<$mt:AuthorDisplayName encode_html=\"1\"$>のアクション</a>\n</div>\n\n<div id=\"profile-comment-threads\" class=\"tab_content hidden\"> \n    <h3 class=\"tab_content_label\">コメントスレッド</h3>\n\n<mt:AuthorCommentResponses lastn=\"5\">  \n    <mt:CommentsHeader>  \n    <ul class=\"recent-actions\">  \n    </mt:CommentsHeader> \n        <li class=\"comment icon-comment\">\n            <a href=\"<$mt:Var name=\"profile_view_url\" encode_html=\"1\"$><$mt:CommenterID$>\"><$mt:CommenterName encode_html=\"1\"$></a>のコメント: \n                <mt:CommentEntry><a href=\"<$mt:CommentLink$>\"><$mt:EntryTitle encode_html=\"1\"$></a></mt:CommentEntry>\n            <div class=\"excerpt\"><$mt:CommentBody words=\"100\"$>...</div>\n    <mt:CommentsFooter> \n    </ul> \n    </mt:CommentsFooter>  \n<mt:Else> \n    <p class=\"note\">コメントへの返信がありません。</p> \n</mt:AuthorCommentResponses>  \n<img src=\"<$mt:StaticWebPath$>images/status_icons/feed.gif\" alt=\"購読する\" /> <a href=\"<$mt:CGIPath$><$mt:CommunityScript$>?__mode=feed&_type=replies&blog_id=<$mt:BlogID$>&id=<$mt:AuthorID$>\"><$mt:AuthorDisplayName encode_html=\"1\"$>のコメントへの返信</a>\n\n</div>\n\n<mt:SetVarBlock name=\"profile_widgets\">\n<div class=\"widget-following widget\">\n    <h3 class=\"widget-header\">注目</h3>\n    <div class=\"widget-content\">\n<mt:AuthorFollowing>\n    <mt:If name=\"__first__\">\n        <ul class=\"user-listing\">\n    </mt:If>\n            <li class=\"pkg\">\n                <$mt:Include module=\"プロフィール画像\" userpic_size=\"36\" userpic_type=\"userlist\"$>\n                <a href=\"<$mt:Var name=\"profile_view_url\" encode_html=\"1\"$><$mt:AuthorID$>\" class=\"user-name\"><$mt:AuthorDisplayName encode_html=\"1\"$></a>\n                <a href=\"<$mt:AuthorURL$>\" class=\"user-url\"><$mt:AuthorURL regex_replace=\"/^\\w+:\\/\\//\",\"\" $></a>\n            </li>\n    <mt:If name=\"__last__\">\n        </ul>\n    </mt:If>\n<mt:Else>\n    <p class=\"note\">まだ誰にも注目していません。</p>\n</mt:AuthorFollowing>\n    </div>\n</div>\n<mt:if name=\"profile_self\">\n    <div class=\"widget-followers widget\">\n        <h3 class=\"widget-header\">被注目</h3>\n        <div class=\"widget-content\">\n    <mt:AuthorFollowers>\n        <mt:If name=\"__first__\">\n            <ul class=\"user-listing\">\n        </mt:If>\n                <li class=\"pkg\">\n                    <$mt:Include module=\"プロフィール画像\" userpic_size=\"36\" userpic_type=\"userlist\"$>\n                    <a href=\"<$mt:Var name=\"profile_view_url\" encode_html=\"1\"$><$mt:AuthorID$>\" class=\"user-name\"><$mt:AuthorDisplayName encode_html=\"1\"$></a>\n                    <a href=\"<$mt:AuthorURL$>\" class=\"user-url\"><$mt:AuthorURL regex_replace=\"/^\\w+:\\/\\//\",\"\" $></a>\n                </li>\n        <mt:If name=\"__last__\">\n            </ul>\n        </mt:If>\n    <mt:Else>\n        <p class=\"note\">まだ注目されていないようです。</p>\n    </mt:AuthorFollowers>\n        </div>\n    </div>\n</mt:if>\n</mt:SetVarBlock>\n\n<mt:IfBlog>\n<$mt:Include module=\"フッター\"$>\n<mt:Else>\n<$mt:Include module=\"フッター\" hide_sidebar=\"1\"$>\n</mt:IfBlog>\n\n','profile_view',0);
INSERT INTO `mt_template` VALUES (39,0,0,0,1,1,'2012-02-08 05:45:55','notify-entry',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','ブログ記事の共有',NULL,1,'新しい<$mt:EntryClassLabel$>「<$mt:EntryTitle$>」を<$mt:BlogName$>で公開しました。\n\n<mt:If name=\"object_type\" eq=\"entry\">表示する<mt:Else>表示する</mt:If> <<mt:SetVarBlock name=\"entry_status\"><$mt:EntryStatus$></mt:SetVarBlock><mt:If name=\"entry_status\" eq=\"Publish\"><$mt:EntryPermalink$><mt:Else><$mt:Var name=\"entry_editurl\"$></mt:If>>\n\n------------------------------------------------------------------------\nタイトル: <$mt:EntryTitle$>\n日付: <$mt:EntryDate mail=\"1\"$>\n<mt:If name=\"send_excerpt\">\n\n<$mt:EntryExcerpt wrap_text=\"72\"$>\n</mt:If>\n------------------------------------------------------------------------\n\n<mt:If name=\"message\">メッセージ: \n<$mt:Var name=\"message\"$></mt:If>\n<mt:If name=\"send_body\">\n------------------------------------------------------------------------\n\n<$mt:EntryBody remove_html=\"1\" wrap_text=\"72\"$>\n\n------------------------------------------------------------------------</mt:If>\n\nこのメールは<$mt:BlogName$>で新規に作成されたコンテンツに関する通知を送るように設定されているか、またはコンテンツの著者が選択したユーザーに送信されています。このメールを受信したくない場合は、次のユーザーに連絡してください:\n\n<$mt:AuthorDisplayName$> <<$mt:AuthorEmail$>>\n','email',0);
INSERT INTO `mt_template` VALUES (40,0,0,0,1,1,'2012-02-08 05:45:55','recover-password',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','パスワードの再設定',NULL,1,'パスワードをリセットしようとしています。以下のリンクをクリックして、新しいパスワードを設定してください。\n\n<mt:var name=\"link_to_login\">\n\nこのメールに心当たりがないときは、何もせずに無視してください。\n\n<mt:include module=\"メールフッター\">\n','email',0);
INSERT INTO `mt_template` VALUES (41,0,0,0,1,1,'2012-02-08 05:45:55','profile_error',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','プロフィールエラー',NULL,1,'\n<$mt:Var name=\"body_class\" value=\"mt-profile-error\"$>\n<mt:SetVarBlock name=\"page_title\">プロフィールエラー</mt:SetVarBlock>\n<mt:SetVarBlock name=\"title\"><mt:If name=\"blog_id\"><$mt:BlogName encode_html=\"1\"$>: </mt:If><$mt:Var name=\"page_title\"$></mt:SetVarBlock>\n\n<$mt:Include module=\"ヘッダー\"$>\n\n<h1 id=\"page-title\" class=\"asset-name\"><$mt:Var name=\"page_title\"$></h1>\n\n<mt:SetVarBlock name=\"status-msg\">＊エラーメッセージを記述してください＊</mt:SetVarBlock>\n<$mt:Include module=\"ステータスメッセージ\" id=\"generic-error\" class=\"error\"$>\n\n<p><a href=\"javascript:void(0)\" onclick=\"history.back()\">戻る</a></p>\n\n<$mt:Include module=\"フッター\"$>\n\n','profile_error',0);
INSERT INTO `mt_template` VALUES (42,0,0,0,1,1,'2012-02-08 05:45:55','status_message',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','ステータスメッセージ',NULL,1,'<div id=\"<$mt:Var name=\"id\"$>\" class=\"<$mt:Var name=\"class\"$>\"><$mt:Var name=\"status-msg\"$></div>\n','custom',0);
INSERT INTO `mt_template` VALUES (43,0,0,0,1,1,'2012-02-08 05:45:55','email_verification_email',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','メールアドレスの確認',NULL,1,'\n<$mt:BlogName$>にご登録いただきありがとうございます。 \n\nセキュリティおよび不正利用を防ぐ観点から、アカウントとメールアドレスの確認をお願いしています。確認され次第、<$mt:BlogName$>にサインインできるようになります。\n\nアカウントの確認のため、次のURLをクリックするか、コピーしてブラウザのアドレス欄に貼り付けてください。\n\n<$mt:Var name=\"confirm_url\"$>\n\nこのメールに覚えがない場合や、<$mt:BlogName$>に登録するのをやめたい場合は、何もする必要はありません。\n\nご協力ありがとうございます。\n\n \n<$mt:BlogName$>\n\n<$mt:Include module=\"メールフッター\"$>\n\n','email',0);
INSERT INTO `mt_template` VALUES (44,0,0,0,1,1,'2012-02-08 05:45:55','javascript',NULL,NULL,NULL,NULL,'2012-02-08 05:45:55','GlobalJavaScript',NULL,1,'\n<mt:Ignore>\n/***\n * Movable Type Community Solution Functions\n */\n</mt:Ignore>\n\nfunction conditional_block(cond, id) {\n    var true_block = document.getElementById(id);\n    var false_block = document.getElementById(id + \'_else\');\n    if (cond) {\n        if (true_block) {\n            var display = true_block.getAttribute(\'mt:display_style\');\n            if (!display && false_block)\n                display = false_block.getAttribute(\'mt:display_style\');\n            if (!display) display = \'\';\n            true_block.style.display = display;\n            DOM.removeClassName(true_block, \'hidden\');\n        }\n        if (false_block) {\n            false_block.style.display = \'none\';\n            DOM.addClassName(false_block, \'hidden\');\n        }\n    }\n    else {\n        if (true_block) {\n            true_block.style.display = \'none\';\n            DOM.addClassName(true_block, \'hidden\');\n        }\n        if (false_block) {\n            var display = false_block.getAttribute(\'mt:display_style\');\n            if (!display && true_block)\n                display = false_block.getAttribute(\'mt:display_style\');\n            if (!display) display = \'\';\n            false_block.style.display = display;\n            DOM.removeClassName(false_block, \'hidden\');\n        }\n    }\n}\n\nif(!this.JSON){JSON={};}(function(){function f(n){return n<10?\'0\'+n:n;}if(typeof Date.prototype.toJSON!==\'function\'){Date.prototype.toJSON=function(key){return this.getUTCFullYear()+\'-\'+f(this.getUTCMonth()+1)+\'-\'+f(this.getUTCDate())+\'T\'+f(this.getUTCHours())+\':\'+f(this.getUTCMinutes())+\':\'+f(this.getUTCSeconds())+\'Z\';};String.prototype.toJSON=Number.prototype.toJSON=Boolean.prototype.toJSON=function(key){return this.valueOf();};}var cx=/[\\u0000\\u00ad\\u0600-\\u0604\\u070f\\u17b4\\u17